        <Field Name="RemainService">
          <DataField>RemainService</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Service_key">
          <DataField>Service_key</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SELECT 
   DD.FullDate
 , DD.YearNum
 , DD.MonthNum
 , DD.MonthNameRus
 , DD.WeekNum
 , DD.WeekName
 , DD.DayNum
 , DD.WeekDayNameRus
 , DD.QuarterNum
 , DD.IsLastDayOfMonth
 , DS.ServiceGroup
 , DS.ServiceName
 , FRS.RemainService
 , FRS.Service_key
FROM dbo.FactRemainService FRS
 JOIN dbo.DimDate DD ON FRS.Date_key = DD.Date_key
 JOIN dbo.DimService DS ON FRS.Service_key = DS.Service_key
WHERE DD.FullDate BETWEEN @StartDate AND @EndDate
 AND (DD.DayNum = 1 OR DD.FullDate = @EndDate);</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
