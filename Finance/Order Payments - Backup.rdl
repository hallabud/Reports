      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <CommandText>WITH C_Accounts (OrderID, InvoiceSum, PaymentSum) AS
 (
SELECT 
   A.OrderID
 , SUM(A.AccSum)
 , SUM(ISNULL(AP.PaySum,0))
FROM dbo.Order_Acc A
 LEFT JOIN dbo.Order_AccPayment AP ON A.ID = AP.AccID AND A.[Year] = AP.AccYear
GROUP BY A.OrderID
 )
, C_Orders AS
 (
SELECT 
   O.AddDate AS OrderAddDate
 , (SELECT MIN(ActivationStartDate) FROM OrderDetail OD WHERE OD.OrderID = O.ID) AS OrderActivationDate
 , O.ID AS OrderID
 , O.NotebookID AS NotebookID
 , NC.Name AS CompanyName
 , M.Name AS ManagerName
 , D1.Name AS OrderState
 , D2.Name AS OrderType 
 , AB.Name AS ActivationBasis
 , O.CatalogPriceSum AS CatalogPrice
 , O.ClientPriceSum AS ClientPrice
 , A.InvoiceSum
 , A.PaymentSum
FROM dbo.Orders O
 JOIN dbo.Directory D1 ON O.State = D1.ID AND D1.Type = 'OrderState'
 JOIN dbo.Directory D2 ON O.Type = D2.ID AND D2.Type = 'OrderType'
 JOIN dbo.Order_ActivationBasis AB ON O.ActivationBasisID = AB.ID
 JOIN dbo.NotebookCompany NC ON O.NotebookID = NC.NotebookID
 JOIN dbo.Manager M ON NC.ManagerID = M.ID
 LEFT JOIN C_Accounts A ON O.ID = A.OrderID
WHERE O.NotebookID NOT IN (853455, 1448295) /*не включаем тестовые компании*/
 AND O.State IN (4,5) /*выбираем только активированные или выполненные заказы*/ 
 )
SELECT 
   DD.FullDate
 , DD.YearNum
 , DD.MonthNum
 , DD.MonthNameRus
 , DD.QuarterNum
 , DD.WeekNum
 , DD.WeekName
 , DD.WeekDayNum
 , DD.WeekDayNameRus
 , C.OrderID
 , C.NotebookID
 , C.CompanyName
 , C.ManagerName
 , C.OrderState
 , C.OrderType 
 , C.ActivationBasis
 , C.CatalogPrice
 , C.ClientPrice
 , C.InvoiceSum
 , C.PaymentSum
FROM C_Orders C
 JOIN Reporting.dbo.DimDate DD ON C.OrderActivationDate = DD.FullDate
WHERE OrderActivationDate BETWEEN @StartDate AND @EndDate</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
