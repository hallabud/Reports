  <DataSets>
    <DataSet Name="DStHotPublicationsSpent">
      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH C AS
 (
SELECT 
   D.YearNum
 , D.MonthNum
 , D.MonthNameEng
 , CASE WHEN O.AddDate &lt;= '2015-07-30' THEN 'до 30.07.2015' ELSE 'с 01.08.2015' END AS OrderGroupByDate 
 , OS.PackageCount
 , NCS.SpendCount
FROM dbo.NotebookCompany_Spent NCS
 JOIN dbo.Orders O ON NCS.OrderID = O.ID
 JOIN dbo.TicketPayment TP ON NCS.TicketPaymentID = TP.Id
 JOIN dbo.OrderDetail OD ON TP.OrderDetailID = OD.ID
 JOIN dbo.OrderService OS ON OD.ServiceID = OS.ID
 JOIN Reporting.dbo.DimDate D ON CAST(NCS.AddDate AS DATE) = D.FullDate
WHERE NCS.SpendType = 5
 AND NCS.AddDate &gt;= @StartDate
 )
SELECT YearNum, MonthNum, MonthNameEng, OrderGroupByDate, PackageCount, SUM(SpendCount) AS SpendCount
FROM C
GROUP BY YearNum, MonthNum, MonthNameEng, OrderGroupByDate, PackageCount
 ;</CommandText>
      </Query>
      <Fields>
        <Field Name="YearNum">
          <DataField>YearNum</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="MonthNum">
          <DataField>MonthNum</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="MonthNameEng">
          <DataField>MonthNameEng</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderGroupByDate">
          <DataField>OrderGroupByDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PackageCount">
          <DataField>PackageCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SpendCount">
          <DataField>SpendCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
