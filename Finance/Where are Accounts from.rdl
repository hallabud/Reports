  <DataSets>
    <DataSet Name="DStAccountsSources">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH C AS 
 (
SELECT 
	CASE
	 WHEN LEN(CONVERT(VARCHAR,ID)) &lt; 5 THEN '1C'
	 WHEN LEN(CONVERT(VARCHAR,ID)) between 5 and 8 THEN 'eCommerce'
	 WHEN SUBSTRING(CONVERT(VARCHAR,ID),1,1) = 2 THEN 'CRM'
	END AS AccountSource,
	[Year] AS AccYear,
	[ID] AS AccID,
	AccSum,
	AddDate,
	DD.FullDate,
	DD.WeekNum,
	DD.WeekName,
	DD.MonthNum,
	DD.MonthNameRus,
	DD.YearNum
FROM SRV16.RabotaUA2.dbo.Order_Acc OA
 JOIN dbo.DimDate DD ON CONVERT(DATE, OA.AddDate) = DD.FullDate
WHERE OA.AddDate BETWEEN @StartDate AND @EndDate + 1
 )
SELECT YearNum, MonthNameRus, MonthNum, AccountSource, COUNT(*) AS AccountsCount
FROM C
GROUP BY YearNum, MonthNameRus, MonthNum, AccountSource;</CommandText>
      </Query>
      <Fields>
        <Field Name="AccountSource">
          <DataField>AccountSource</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AccountsCount">
          <DataField>AccountsCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MonthNum">
          <DataField>MonthNum</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="MonthNameRus">
          <DataField>MonthNameRus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="YearNum">
          <DataField>YearNum</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
