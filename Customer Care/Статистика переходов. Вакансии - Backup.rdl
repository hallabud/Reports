  <DataSets>
    <DataSet Name="VacancyViewsDataset">
      <Fields>
        <Field Name="Дата">
          <DataField>Дата</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Количество_переходов">
          <DataField>Количество переходов</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Название_вакансии">
          <DataField>Название вакансии</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Название_компании">
          <DataField>Название компании</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SELECT
 CONVERT(NVARCHAR(30),DATEADD(DAY,[Day],'2006-01-01'),104) AS 'Дата'
 , Detail AS 'Количество переходов'
 , V.Name + ' (ID = ' + CAST( V.ID AS VARCHAR) + ')' AS 'Название вакансии'
 , NC.Name AS 'Название компании'
FROM SRV12.RabotaUA2.dbo.MegaVIPVacancyStatistic VS
 JOIN SRV12.RabotaUA2.dbo.Vacancy V
  ON VS.VacancyID = V.Id
 JOIN SRV12.RabotaUA2.dbo.NotebookCompany NC
  ON NC.NotebookID = V.NotebookID
WHERE V.NotebookId = @NotebookCompanyID
 AND DATEADD(DAY,[Day],'2006-01-01') BETWEEN @StartDate AND @EndDate
ORDER BY DATEADD(DAY,[Day],'2006-01-01');</CommandText>
        <QueryParameters>
          <QueryParameter Name="@NotebookCompanyID">
            <Value>=Parameters!NotebookCompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ImagesDataset">
      <Fields>
        <Field Name="Logo">
          <DataField>Logo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SELECT Logo 
FROM SRV12.RabotaUA2.dbo.NotebookCompany 
WHERE NotebookId = @NotebookCompanyId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@NotebookCompanyId">
            <Value>=Parameters!NotebookCompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
