  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DSAzure">
      <DataSourceReference>AZURE</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>dd877d95-be8b-4872-9fff-6a047db2bd38</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DStTableSizes">
      <Query>
        <DataSourceName>DSAzure</DataSourceName>
        <CommandText>if object_id('tempdb..#object_pages','U') is not null drop table #object_pages;
select
	ps.object_id,
    sum(case when (ps.index_id &lt; 2) then row_count else 0 end ) as [rows],
    sum(ps.reserved_page_count) as reserved,
    sum(case when (ps.index_id &lt; 2) then (ps.in_row_data_page_count + ps.lob_used_page_count + ps.row_overflow_used_page_count) else (ps.lob_used_page_count + ps.row_overflow_used_page_count) end) as data,
	sum(ps.used_page_count) as used
into #object_pages
from sys.dm_db_partition_stats ps
group by ps.object_id

;with result as (
select
	sch.name as schemaname,
	ao.name as tablename,
	op.rows as row_count,
	op.reserved * 8.0 / 1024 as reserved_mb,
	op.data * 8.0 / 1024 as data_mb,
	(case when op.used &gt; op.data then op.used - op.data else 0 end) * 8.0 / 1024 as index_size_mb,
	(case when op.reserved &gt; op.used then op.reserved - op.used else 0 end) * 8.0 / 1024 as unused_mb,
	row_number() over(order by op.reserved desc) rn
from #object_pages as op join sys.all_objects ao on op.object_id = ao.object_id
						 join sys.schemas sch on ao.schema_id = sch.schema_id
where ao.type != N'S' and ao.type != N'IT')
select
	schemaname,
	tablename,
	row_count,
	reserved_mb,
	data_mb,
	index_size_mb,
	unused_mb,
	case when rn &gt;= 11 then 11 else rn end rn,
case when rn &gt;= 11 then 'Other' else schemaname+'.'+tablename end rn_tablename
from result
order by reserved_mb desc         </CommandText>
      </Query>
