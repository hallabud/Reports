      <Fields>
        <Field Name="NotebookId">
          <DataField>NotebookId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Phone">
          <DataField>Phone</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="VacancyURL">
          <DataField>VacancyURL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="VacancyName">
          <DataField>VacancyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="VacancyState">
          <DataField>VacancyState</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <CommandText>WITH CTE AS
 (
SELECT
   NC.NotebookId
 , NC.AddDate AS RegDate
 , NCS.Id AS SpentId
 , NCS.AddDate AS SpentDate
 , 'http://rabota.ua/company' + CAST(NC.NotebookId AS VARCHAR) + '/vacancy' + CAST(NCS.VacancyID AS VARCHAR) AS VacancyURL
 , MU.Name AS UserName
 , MU.Phone
 , V.Name AS VacancyName
 , VS.Name AS VacancyState
 , ROW_NUMBER() OVER(PARTITION BY NC.NotebookId ORDER BY NCS.Id) AS SpentOrderNum
FROM dbo.NotebookCompany NC
 JOIN dbo.NotebookCompany_Spent NCS ON NC.NotebookId = NCS.NotebookId
 JOIN dbo.MultiUser MU ON NCS.MultiUserId = MU.ID
 JOIN dbo.Vacancy V ON NCS.VacancyId = V.Id
 JOIN dbo.VacancyState VS ON V.State = VS.Id
WHERE NC.IsLegalPerson = 1
 AND NC.AddDate &gt;= DATEADD(MONTH,-1,dbo.fnGetDatePart(GETDATE()))
 AND NOT EXISTS (SELECT * FROM dbo.NotebookCompanyMerged NCM WHERE NCM.SourceNotebookId = NC.NotebookId)
 )
SELECT NotebookId, UserName, Phone, VacancyURL, VacancyName, VacancyState
FROM CTE 
WHERE SpentOrderNum = 1
 AND DATEDIFF(DAY, SpentDate, GETDATE()) = 14;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
