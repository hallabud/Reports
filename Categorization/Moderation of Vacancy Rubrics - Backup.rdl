    <DataSet Name="DstCategorizators">
      <Fields>
        <Field Name="ModeratedRubricLastLogin">
          <DataField>ModeratedRubricLastLogin</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SELECT DISTINCT ModeratedRubricLastLogin 
FROM srv12.RabotaUA2.dbo.VacancyExtra 
WHERE ISNULL(ModeratedRubricLastLogin,'') &lt;&gt; ''
 AND ModeratedRubricLastDate BETWEEN @StartDate AND DATEADD(DAY, 1, @EndDate)
ORDER BY ModeratedRubricLastLogin ;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
