  <DataSources>
    <DataSource Name="DSRcReporting">
      <Transaction>true</Transaction>
      <DataSourceReference>DSrcReporting</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>fa601e17-e2bb-4df6-9075-33e8c928b90a</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DStMain">
      <Query>
        <DataSourceName>DSRcReporting</DataSourceName>
        <CommandText>WITH C AS
 (
SELECT SG.SynonymousID, SG.Name AS TagGroup, C.Id AS CityID, C.Name AS CityName
FROM SRV16.RabotaUA2.dbo.SynonymGroup SG WITH (NOLOCK)
 CROSS JOIN SRV16.RabotaUA2.dbo.City C WITH (NOLOCK)
WHERE C.Name IN ('Киев','Харьков','Днепропетровск','Одесса')
 )
 , C2 AS
 (
SELECT * 
 , (SELECT COUNT(*)
    FROM SRV16.RabotaUA2.dbo.VacancySubscriptionActual VSA WITH (NOLOCK)
	WHERE CHARINDEX(',' + CAST(C.SynonymousID AS VARCHAR) + ',', ',' + VSA.SynonymousIDs + ',') &gt; 0
	 AND CHARINDEX(',' + CAST(C.CityID AS VARCHAR) + ',', ',' + VSA.CityIDs + ',') &gt; 0) AS VacancyCount
FROM C
 )
SELECT * 
FROM C2
WHERE VacancyCount &gt; 20
ORDER BY VacancyCount DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="SynonymousID">
          <DataField>SynonymousID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TagGroup">
          <DataField>TagGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CityID">
          <DataField>CityID</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="CityName">
          <DataField>CityName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="VacancyCount">
          <DataField>VacancyCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DStPoolDate">
      <Query>
        <DataSourceName>DSRcReporting</DataSourceName>
        <CommandText>SELECT MAX(dbo.fnGetDatePart(SubscriptionDate)) AS PoolDate
FROM SRV16.RabotaUA2.dbo.VacancySubscriptionActual WITH (NOLOCK)</CommandText>
      </Query>
      <Fields>
        <Field Name="PoolDate">
          <DataField>PoolDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
