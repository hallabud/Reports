    <DataSet Name="DStMain">
      <Fields>
        <Field Name="NotebookID">
          <DataField>NotebookID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>DECLARE @TodayDate DATETIME;
DECLARE @CurFirstMonthDate DATETIME;
DECLARE @PrvFirstMonthDate DATETIME;

SET @TodayDate = Analytics.dbo.fnGetDatePart(GETDATE());
SET @CurFirstMonthDate = DATEADD(DAY, 1 - DATEPART(DAY, @TodayDate), @TodayDate);
SET @PrvFirstMonthDate = DATEADD(MONTH, -1, @CurFirstMonthDate);

SELECT NC.NotebookID, NC.Name
FROM Analytics.dbo.NotebookCompany NC
 JOIN Analytics.dbo.Notebook N ON NC.NotebookID = N.Id
WHERE EXISTS (SELECT * FROM Analytics.dbo.NotebookCompany_Spent NCS WHERE NCS.NotebookID = NC.NotebookID AND NCS.AddDate &gt;= @CurFirstMonthDate)
 AND EXISTS (SELECT * FROM Analytics.dbo.NotebookCompany_Spent NCS WHERE NCS.NotebookID = NC.NotebookID AND NCS.AddDate BETWEEN @PrvFirstMonthDate AND @CurFirstMonthDate)
 AND NC.IsLegalPerson = 1 AND N.NotebookStateID = 5;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
