    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DStMain">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ModeOrd">
            <Value>=Parameters!orderCnt.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@ModePaid">
            <Value>=Parameters!paidCnt.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@amountOfDays">
            <Value>=Parameters!amountOfDays.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT m.Admin2_InformingLogin [Login], ModeratorName, COUNT(DISTINCT m.notebookid) Informed,
	   CASE WHEN @ModeOrd = 1 
			THEN SUM(orderEcom)
			WHEN @ModeOrd = 2
			THEN SUM(orderMng)
			ELSE COUNT(OrderID)
			END orderCnt,

	   CASE WHEN @ModePaid = 3 
			THEN SUM(PaidOrderEcom)
			WHEN @ModePaid = 4
			THEN SUM(PaidOrderMng)
			ELSE COUNT(paimentID)
			END paidCnt
FROM
(	SELECT nce.Admin2_InformingLogin [Admin2_InformingLogin], m.Name [ModeratorName], nce.Admin2_InformingDate, nce.notebookid [notebookid], o.id [orderID], o.adddate [OrderDate], oap.id [paimentID], oap.adddate PaymentDate,
	DATEDIFF(day, nce.Admin2_InformingDate, o.adddate) AS DateDiffOrd, DATEDIFF(day, nce.Admin2_InformingDate, oap.adddate) AS DateDiffPayment,

		   case when o.type=5 then 1 else 0 end orderEcom,
		   case when o.type&lt;&gt;5 then 1 else 0 end orderMng,
		   case when o.type=5 and oap.id is NOT NULL then 1 else 0 end PaidOrderEcom,
		   case when o.type&lt;&gt;5 and oap.id is NOT NULL then 1 else 0 end PaidOrderMng
	
	FROM srv16.rabotaua2.dbo.NotebookCompanyExtra nce with(nolock) 
	LEFT JOIN srv16.rabotaua2.dbo.Orders o with(nolock) ON nce.notebookid=o.notebookid and nce.Admin2_IsInformed=1
	LEFT JOIN srv16.rabotaua2.dbo.Order_Acc oa with(nolock)  ON o.id=oa.orderid and oa.adddate between nce.Admin2_InformingDate and dateadd(d,@amountOfDays,nce.Admin2_InformingDate)
	LEFT JOIN srv16.rabotaua2.dbo.Order_AccPayment oap with(nolock) ON oap.accid=oa.id AND oap.AccYear = oa.Year 
																   --AND oap.adddate between @StartDate and @EndDate 
																   AND oap.adddate between nce.Admin2_InformingDate and dateadd(d,@amountOfDays,nce.Admin2_InformingDate)
	JOIN srv16.rabotaua2.dbo.Manager m with(nolock) ON m.Email=nce.Admin2_InformingLogin and m.departmentid = 8
													--order by Admin2_InformingLogin
	WHERE nce.Admin2_InformingDate between @StartDate and dateadd(d, +1, @EndDate)

) m

GROUP BY m.Admin2_InformingLogin, m.ModeratorName
order by Login</CommandText>
      </Query>
      <Fields>
        <Field Name="Login">
          <DataField>Login</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Informed">
          <DataField>Informed</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="orderCnt">
          <DataField>orderCnt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="paidCnt">
          <DataField>paidCnt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ModeratorName">
          <DataField>ModeratorName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DStUsers">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>select
	id,
	name,
	Email
from srv16.rabotaua2.dbo.Manager with(nolock)
where departmentid = 8
union all
select
	id,
	name,
	Email
from srv16.rabotaua2.dbo.Manager with(nolock)
where id = 310
order by name</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Email">
          <DataField>Email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
