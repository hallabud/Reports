  <DataSets>
    <DataSet Name="DStAcquisition">
      <Fields>
        <Field Name="NotebookStateName">
          <DataField>NotebookStateName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IsHasDoubt">
          <DataField>IsHasDoubt</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LegacyState">
          <DataField>LegacyState</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BlockingReason">
          <DataField>BlockingReason</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompanyNum">
          <DataField>CompanyNum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>WITH C AS
 (
SELECT 
   CASE 
    WHEN NS.ID IN (5,7) THEN 'проверенные + МП'
	WHEN EXISTS (SELECT * FROM Analytics.dbo.NotebookCompanyMerged NCM WHERE NCM.SourceNotebookID = NC.NotebookID) THEN 'присоединены к другому блокноту'
	ELSE NS.Name 
   END AS 'NotebookStateName'
 , CASE NC.IsHasDoubt WHEN 1 THEN 'Сомнительная компания' END AS 'IsHasDoubt'
 , CASE NC.IsLegalPerson WHEN 1 THEN 'Юрлицо' WHEN 0 THEN 'Физлицо' END AS 'LegacyState'
 , CASE 
    WHEN NC.BlockingReasonIDs2 = '23' THEN 'Отказ по коду'
    WHEN NC.BlockingReasonIDs2 = '20' THEN 'Отказ по контактам'
    WHEN NC.BlockingReasonIDs2 = '51' THEN 'Кастом-отказ'
    WHEN NC.BlockingReasonIDs2 LIKE '%,%' THEN 'Несколько причин' 
   END AS 'BlockingReason' 
 , NC.NotebookID
FROM Analytics.dbo.NotebookCompany NC
 JOIN Analytics.dbo.Notebook N ON NC.NotebookId = N.Id
 JOIN Analytics.dbo.NotebookState NS ON N.NotebookStateId = NS.Id
WHERE NC.AddDate BETWEEN @StartDate AND @EndDate + 1
 AND NC.IsConfirmed = 1
 )
SELECT 
   NotebookStateName
 , IsHasDoubt
 , LegacyState
 , BlockingReason
 , COUNT(*) AS CompanyNum
FROM C 
GROUP BY
   NotebookStateName
 , IsHasDoubt
 , LegacyState
 , BlockingReason;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
