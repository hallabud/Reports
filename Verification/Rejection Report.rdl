    <DataSet Name="DStModerators">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

SELECT DISTINCT LoginEmail, REPLACE(NE.Name + ' ' + NE.SurName, '  ', ' ') AS ModeratorName
FROM SRV16.RabotaUA2.dbo.NotebookCompany_Log NCL
 JOIN SRV16.RabotaUA2.dbo.aspnet_Membership MMB ON NCL.LoginEmail = MMB.Email
 JOIN SRV16.RabotaUA2.dbo.Notebook N ON MMB.UserId = N.aspnet_UserUIN
 JOIN SRV16.RabotaUA2.dbo.NotebookEmployee NE ON N.ID = NE.NotebookId
WHERE ValStr &lt;&gt; ''
 AND NCL.AddDate BETWEEN @StartDate AND @EndDate + 1;</CommandText>
      </Query>
      <Fields>
        <Field Name="LoginEmail">
          <DataField>LoginEmail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ModeratorName">
          <DataField>ModeratorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
