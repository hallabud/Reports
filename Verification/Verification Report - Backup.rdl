 , C.NotebookId
 , MAX(DATEDIFF(MINUTE,AddDate,AddDate2)) AS Minutes
FROM C
 JOIN C_MaxLogs ML ON C.NotebookId = ML.NotebookId AND C.LogId2 = ML.MaxLogId2
GROUP BY 
   dbo.fnGetDatePart(AddDate)
 , CASE
