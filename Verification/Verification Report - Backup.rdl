    <DataSet Name="DstVerificationTotals">
      <Fields>
        <Field Name="FullDate">
          <DataField>FullDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="VerificationResult">
          <DataField>VerificationResult</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="VerificationResultDetails">
          <DataField>VerificationResultDetails</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompaniesNum">
          <DataField>CompaniesNum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>WITH C AS
 (
SELECT 
   ROW_NUMBER() OVER(PARTITION BY L1.LogID ORDER BY L2.AddDate) AS AfterConfirmActionNum
 , L1.*
 , L2.LogID AS LogID2
 , L2.EventID AS EventID2
 , L2.PageID AS PageID2
 , L2.LoginEmail AS LoginEmail2
 , L2.AddDate AS AddDate2
 , L2.ValInt AS ValInt2
 , L2.ValStr AS ValStr2
 , L2.ValBit AS ValBit2
FROM SRV12.RabotaUA2.dbo.NotebookCompany_Log L1
 LEFT JOIN SRV12.RabotaUA2.dbo.NotebookCompany_Log L2 
  ON L1.NotebookID = L2.NotebookID 
   AND L2.EventID &lt;&gt; 1 
   AND L2.ValInt &lt;&gt; 0
   AND L2.AddDate &gt; L1.AddDate
   AND L2.AddDate &lt;= DATEADD(HOUR, 19, DATEADD(DAY,1,dbo.fnGetDatePart(L1.AddDate)))
WHERE L1.EventID = 1
 AND L1.AddDate BETWEEN @StartDate AND @EndDate
 )
, C_Details AS
 (
SELECT 
   dbo.fnGetDatePart(C.AddDate) AS FullDate
 , CASE 
    WHEN EventID2 = 8 THEN 'Присоединение к другому блокноту'
    WHEN LogID2 IS NULL AND ValInt IN (5,7) THEN 'Проверенный + МП' -- автопроверенный с корпоративным емейлом
    WHEN ValInt2 IN (5,7) THEN 'Проверенный + МП'
    WHEN ValInt = 4 OR ValInt2 = 4 THEN 'Черный список'
    WHEN ValStr2 IS NOT NULL THEN 'Отказ'
	ELSE 'Необработанные '
   END AS VerificationResult
 , CASE 
    WHEN ValStr2 = '' AND ISNULL(ValBit2,0) = 1 THEN 'Сомнительная компания'
    WHEN ValStr2 &lt;&gt; '' AND ValStr2 LIKE '%23%' THEN 'По ЕГРПОУ'
    WHEN ValStr2 &lt;&gt; '' AND ValStr2 LIKE '%20%' THEN 'По контактам'
    WHEN ValStr2 &lt;&gt; '' AND ValStr2 LIKE '%51%' THEN 'По другой причине'
    WHEN ValStr2 &lt;&gt; '' THEN 'По неизвестной причине'
   END AS VerificationResultDetails
 , *
FROM C 
WHERE AfterConfirmActionNum = 1
 )

SELECT 
   FullDate
 , VerificationResult
 , VerificationResultDetails
 , COUNT(DISTINCT NotebookId) AS 'CompaniesNum'
FROM C_Details
GROUP BY
   FullDate
 , VerificationResult
 , VerificationResultDetails;</CommandText>
