      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>WITH C AS
 (
SELECT 
   ROW_NUMBER() OVER(PARTITION BY L1.LogID ORDER BY L2.AddDate) AS AfterConfirmActionNum
 , L1.*
 , L2.LogID AS LogID2
 , L2.EventID AS EventID2
 , L2.PageID AS PageID2
 , L2.LoginEmail AS LoginEmail2
 , L2.AddDate AS AddDate2
 , L2.ValInt AS ValInt2
 , L2.ValStr AS ValStr2
 , L2.ValBit AS ValBit2
FROM SRV12.RabotaUA2.dbo.NotebookCompany_Log L1
 LEFT JOIN SRV12.RabotaUA2.dbo.NotebookCompany_Log L2 
  ON L1.NotebookID = L2.NotebookID 
   AND L2.EventID &lt;&gt; 1 
   AND L2.ValInt &lt;&gt; 0
   AND L2.AddDate &gt; L1.AddDate
   AND L2.AddDate &lt;= DATEADD(HOUR, 19, DATEADD(DAY,1,dbo.fnGetDatePart(L1.AddDate)))
WHERE L1.EventID = 1
 AND L1.AddDate BETWEEN @StartDate AND @EndDate
 )
, C_ResultStep1 AS
 (
SELECT 
   dbo.fnGetDatePart(AddDate) AS FullDate
 , CASE 
    WHEN DATEPART(HOUR,AddDate) BETWEEN 9 AND 18 THEN 'Рабочее время'
    ELSE 'Нерабочее время'
   END AS 'IsWorkingHours'
 , CASE 
    WHEN DATEPART(HOUR,AddDate) BETWEEN 9 AND 10 THEN 'с 9-00 до 11-00'
    WHEN DATEPART(HOUR,AddDate) BETWEEN 11 AND 18 THEN 'с 11-00 до 19-00'
    WHEN DATEPART(HOUR,AddDate) BETWEEN 19 AND 23 THEN 'с 19-00 до 24-00'
    WHEN DATEPART(HOUR,AddDate) BETWEEN 0 AND 8 THEN 'с 24-00 до 9-00'
   END AS 'DayPeriod'
 , C.NotebookId
 , MAX(DATEDIFF(MINUTE,AddDate,AddDate2)) AS Minutes
FROM C
WHERE AfterConfirmActionNum = 1
GROUP BY 
   dbo.fnGetDatePart(AddDate)
 , CASE 
    WHEN DATEPART(HOUR,AddDate) BETWEEN 9 AND 18 THEN 'Рабочее время'
    ELSE 'Нерабочее время'
   END
 , CASE 
    WHEN DATEPART(HOUR,AddDate) BETWEEN 9 AND 10 THEN 'с 9-00 до 11-00'
    WHEN DATEPART(HOUR,AddDate) BETWEEN 11 AND 18 THEN 'с 11-00 до 19-00'
    WHEN DATEPART(HOUR,AddDate) BETWEEN 19 AND 23 THEN 'с 19-00 до 24-00'
    WHEN DATEPART(HOUR,AddDate) BETWEEN 0 AND 8 THEN 'с 24-00 до 9-00'
   END
 , C.NotebookId
 )
SELECT 
   FullDate
 , IsWorkingHours
 , DayPeriod
 , CASE 
    WHEN Minutes &lt; 10 THEN 'до 10 мин.'
    WHEN Minutes BETWEEN 10 AND 29 THEN 'от 10 мин. до 30 мин.'
    WHEN Minutes BETWEEN 30 AND 60 THEN 'от 30 мин. до 1 часа'
    WHEN Minutes &gt; 60 THEN 'более 1 часа'
   END AS 'TimePeriod'
 , COUNT(DISTINCT NotebookId) AS CompaniesNum
FROM C_ResultStep1
WHERE Minutes IS NOT NULL
GROUP BY 
   FullDate
 , IsWorkingHours
 , DayPeriod
 , CASE 
    WHEN Minutes &lt; 10 THEN 'до 10 мин.'
    WHEN Minutes BETWEEN 10 AND 29 THEN 'от 10 мин. до 30 мин.'
    WHEN Minutes BETWEEN 30 AND 60 THEN 'от 30 мин. до 1 часа'
    WHEN Minutes &gt; 60 THEN 'более 1 часа'
   END
;</CommandText>
