    <DataSet Name="DStManagersSF_CC">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SELECT Id, Name, AM.Email 
FROM Analytics.dbo.Manager M
 JOIN Analytics.dbo.aspnet_Membership AM ON M.aspnet_UserUIN = AM.UserId
WHERE DepartmentID = 2 AND IsReportExcluding = 1</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
