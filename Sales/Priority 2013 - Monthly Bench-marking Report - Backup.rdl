        <CommandText>SELECT M.Name FROM Analytics.dbo.Manager M WHERE EXISTS (SELECT * FROM Analytics.dbo.NotebookCompany NC WHERE NC.ManagerId = M.Id) AND M.Name NOT IN ('QA','telesales') ORDER BY M.Surname</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
