      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>WITH CTE_Rabota (SiteName, DwFullDate, RealFullDate, RegionGroup, ManagerName, CompaniesNum) AS
 (
SELECT 
   'rabota.ua'
 , D.FullDate
 , DATEADD(DAY, -1, D.FullDate)
 , CASE 
    WHEN R.RegionGroup IN ('Киев','Донецк, Днепр, Харьков, Одесса, Львов') THEN R.NameRus
    ELSE 'Вся Украина за исключением Киева и крупных городов' 
   END
 , M.Name
 , COUNT(*)
FROM dbo.FactCompanyStatuses CS
 JOIN dbo.DimCompany C ON CS.Company_key = C.Company_key
 JOIN dbo.DimRegion R ON C.Region_key = R.Region_key
 JOIN dbo.DimDate D ON CS.Date_key = D.Date_key
 JOIN Analytics.dbo.NotebookCompany NC ON C.NotebookId = NC.NotebookId
 JOIN Analytics.dbo.NotebookCompanyExtra NCE ON NC.NotebookId = NCE.NotebookId
 JOIN Analytics.dbo.Manager M ON NC.ManagerId = M.Id
 JOIN Analytics.dbo.Notebook N ON NC.NotebookId = N.Id
WHERE D.DayNum = 1
 AND D.FullDate &gt; '2013-01-01'
 AND NCE.Admin3_TrophyBrand_IsPriority2013 = 1
 AND (CS.HasPaidPublishedVacs = 1 OR CS.HasPaidPublicationsLeft = 1 OR CS.HasHotPublishedVacs = 1 OR CS.HasHotPublicationsLeft = 1 OR CS.HasCVDBAccess = 1 OR CS.HasActivatedLogo = 1 OR CS.HasActivatedProfile = 1)
 AND N.NotebookStateId IN (5,7)
 AND NOT EXISTS (SELECT * FROM Analytics.dbo.NotebookCompanyMerged NCM WHERE NCM.SourceNotebookId = NC.NotebookId)
GROUP BY 
   D.FullDate
 , DATEADD(DAY, -1, D.FullDate)
 , CASE 
    WHEN R.RegionGroup IN ('Киев','Донецк, Днепр, Харьков, Одесса, Львов') THEN R.NameRus
    ELSE 'Вся Украина за исключением Киева и крупных городов' 
   END
 , M.Name
 )
, CTE_Work (SiteName, DwFullDate, RealFullDate, RegionGroup, ManagerName, CompaniesNum) AS
 (
SELECT 
   'work.ua'
 , DATEADD(DAY,1,D.FullDate)
 , D.FullDate
 , CASE 
    WHEN C.Name IN ('Киев','Донецк', 'Днепропетровск', 'Харьков', 'Одесса', 'Львов') THEN C.Name
    ELSE 'Вся Украина за исключением Киева и крупных городов'
   END
 , M.Name
 , COUNT(DISTINCT SC.NotebookId)
FROM Analytics.dbo.SpiderCompanyHistory SCH
 JOIN Analytics.dbo.SpiderCompany SC ON SCH.SpiderCompanyID = SC.Id
 JOIN Analytics.dbo.NotebookCompany NC ON SC.NotebookId = NC.NotebookID
 JOIN Analytics.dbo.City C ON NC.HeadquarterCityId = C.Id
 JOIN Analytics.dbo.NotebookCompanyExtra NCE ON NC.NotebookId = NCE.NotebookId
 JOIN (SELECT FullDate FROM dbo.DimDate WHERE IsLastDayOfMonth = 1 AND FullDate &gt; '2013-01-01') D
  ON dbo.fnGetDatePart(SCH.AddDate) = D.FullDate
 JOIN Analytics.dbo.Manager M ON NC.ManagerId = M.Id
 JOIN Analytics.dbo.Notebook N ON NC.NotebookId = N.Id
WHERE SC.NotebookId IS NOT NULL
 AND SC.Source = 1 
 AND NCE.Admin3_TrophyBrand_IsPriority2013 = 1
 AND (/*SCH.VacancyCount &gt; 3 OR*/ SCH.Paid = 1 OR SCH.IsPaidByTickets = 1 OR SCH.HotVacancyCount &gt; 0)
 AND N.NotebookStateId IN (5,7)
 AND NOT EXISTS (SELECT * FROM Analytics.dbo.NotebookCompanyMerged NCM WHERE NCM.SourceNotebookId = NC.NotebookId)
GROUP BY
   DATEADD(DAY,1,D.FullDate)
 , D.FullDate
 , CASE 
    WHEN C.Name IN ('Киев','Донецк', 'Днепропетровск', 'Харьков', 'Одесса', 'Львов') THEN C.Name
    ELSE 'Вся Украина за исключением Киева и крупных городов'
   END
 , M.Name
 )
, CTE_Hh (SiteName, DwFullDate, RealFullDate, RegionGroup, ManagerName, CompaniesNum) AS
 (
SELECT 
   'hh.ua'
 , DATEADD(DAY,1,D.FullDate)
 , D.FullDate
 , CASE 
    WHEN C.Name IN ('Киев','Донецк', 'Днепропетровск', 'Харьков', 'Одесса', 'Львов') THEN C.Name
    ELSE 'Вся Украина за исключением Киева и крупных городов'
   END
 , M.Name
 , COUNT(DISTINCT SC.NotebookId)
FROM Analytics.dbo.SpiderCompanyHistory SCH
 JOIN Analytics.dbo.SpiderCompany SC ON SCH.SpiderCompanyID = SC.Id
 JOIN Analytics.dbo.NotebookCompany NC ON SC.NotebookId = NC.NotebookID
 JOIN Analytics.dbo.City C ON NC.HeadquarterCityId = C.Id
 JOIN Analytics.dbo.NotebookCompanyExtra NCE ON NC.NotebookId = NCE.NotebookId
 JOIN (SELECT FullDate FROM dbo.DimDate WHERE IsLastDayOfMonth = 1 AND FullDate &gt; '2013-01-01') D
  ON dbo.fnGetDatePart(SCH.AddDate) = D.FullDate
 JOIN Analytics.dbo.Manager M ON NC.ManagerId = M.Id
 JOIN Analytics.dbo.Notebook N ON NC.NotebookId = N.Id
WHERE SC.NotebookId IS NOT NULL
 AND SC.Source = 2 
 AND NCE.Admin3_TrophyBrand_IsPriority2013 = 1
 AND (SCH.VacancyCount &gt; 0 AND SCH.Paid = 1)
 AND N.NotebookStateId IN (5,7)
 AND NOT EXISTS (SELECT * FROM Analytics.dbo.NotebookCompanyMerged NCM WHERE NCM.SourceNotebookId = NC.NotebookId)
GROUP BY
   DATEADD(DAY,1,D.FullDate)
 , D.FullDate
 , CASE 
    WHEN C.Name IN ('Киев','Донецк', 'Днепропетровск', 'Харьков', 'Одесса', 'Львов') THEN C.Name
    ELSE 'Вся Украина за исключением Киева и крупных городов'
   END
 , M.Name
 )

SELECT *
FROM CTE_Rabota
UNION ALL
SELECT *
FROM CTE_Work
UNION ALL
SELECT *
FROM CTE_Hh</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
