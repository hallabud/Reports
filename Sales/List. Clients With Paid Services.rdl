        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

-- список компаний по датам (1е дни месяца), заданным параметрами процедуры (между @StartDate и @EndDate)
-- + предрасчитваем даты: "Текущая дата - 1 мес." и "Текущая дата - 1 год"
IF OBJECT_ID('tempdb..#СompaniesListByDates','U') IS NOT NULL DROP TABLE #СompaniesListByDates;

SELECT
   CS.Company_key
 , NC.NotebookID
 , NC.Name AS 'CompanyName'
 , M.Name AS 'ManagerName'
 , DD.FullDate
 , CASE WHEN DD.DayNum = 1 THEN DATEADD(MONTH, -1, DD.FullDate) ELSE DD.FirstMonthDate END AS FullDate1MonthAgoOrFirstMonthDate
 , DATEADD(YEAR, -1, DD.FullDate) AS FullDate1YearAgo
 , DD.Date_key
 , CASE 
    WHEN HasPaidPublishedVacs | HasPaidPublicationsLeft = 1 THEN 'Есть платные публикации на счету или опубликованные платные вакансии'
    WHEN HasHotPublishedVacs | HasHotPublicationsLeft = 1 THEN 'Есть горячие публикации на счету или опубликованные горячие вакансии'
	WHEN HasCVDBAccess = 1 THEN 'Есть доступ к базе резюме'
	WHEN HasActivatedLogo | HasActivatedProfile = 1 THEN 'Клиенты по другому сервису (профиль/логотип)'
    ELSE 'Не использовали платный сервис'
   END AS 'CompanyGroup'
 , CASE 
    WHEN HasPaidPublishedVacs | HasPaidPublicationsLeft = 1 THEN 1
    WHEN HasHotPublishedVacs | HasHotPublicationsLeft = 1 THEN 2
	WHEN HasCVDBAccess = 1 THEN 3
	WHEN HasActivatedLogo = 1 OR HasActivatedProfile = 1 THEN 4
    ELSE 5
   END AS 'CardColor'
 , CS.VacancyNum + ISNULL(CS.UnqWorkVacancyNum,0) AS UnqVacancyNum
 , CS.VacancyNum
INTO #СompaniesListByDates
FROM Reporting.dbo.DimCompany DC
 JOIN SRV16.RabotaUA2.dbo.NotebookCompany NC ON DC.NotebookId = NC.NotebookId
 JOIN SRV16.RabotaUA2.dbo.Manager M ON NC.ManagerId = M.Id
 JOIN Reporting.dbo.FactCompanyStatuses CS ON DC.Company_key = CS.Company_key
 JOIN Reporting.dbo.DimDate DD ON CS.Date_key = DD.Date_key
WHERE DD.FullDate BETWEEN @StartDate AND @EndDate
 AND (DD.DayNum = 1 OR DD.FullDate = @EndDate)
 AND NC.IsNetworkCompany = 0
 AND NOT EXISTS (SELECT * FROM SRV16.RabotaUA2.dbo.NotebookCompanyMerged WHERE SourceNotebookID = NC.NotebookID)
 AND M.DepartmentID IN (2,3)
 AND M.ID IN (@ManagerID)


-- тот же список компаний что и в #СompaniesListByDates, но с наличием любого признака платности за последний год
-- + расчитываем наличия признаков платности за последний месяц

IF OBJECT_ID('tempdb..#CompaniesLimitedList','U') IS NOT NULL DROP TABLE #CompaniesLimitedList;

SELECT * 
 , CASE
    WHEN EXISTS (SELECT * 
	             FROM Reporting.dbo.FactCompanyStatuses CS
	              JOIN dbo.DimDate DD ON CS.Date_key = DD.Date_key
				 WHERE CS.Company_key = CL.Company_key 
				  AND CS.HasPaidPublishedVacs | CS.HasPaidPublicationsLeft = 1 
				  AND DD.FullDate BETWEEN CL.FullDate1MonthAgoOrFirstMonthDate AND CL.FullDate)
	THEN 1 ELSE 0 END AS HasVacancy
 , CASE
    WHEN EXISTS (SELECT * 
	             FROM Reporting.dbo.FactCompanyStatuses CS
	              JOIN dbo.DimDate DD ON CS.Date_key = DD.Date_key
				 WHERE CS.Company_key = CL.Company_key 
				  AND CS.HasHotPublishedVacs | CS.HasHotPublicationsLeft = 1
				  AND DD.FullDate BETWEEN CL.FullDate1MonthAgoOrFirstMonthDate AND CL.FullDate)
	THEN 1 ELSE 0 END AS HasHot
 , CASE
    WHEN EXISTS (SELECT * 
	             FROM Reporting.dbo.FactCompanyStatuses CS
	              JOIN dbo.DimDate DD ON CS.Date_key = DD.Date_key
				 WHERE CS.Company_key = CL.Company_key 
				  AND CS.HasCVDBAccess = 1
				  AND DD.FullDate BETWEEN CL.FullDate1MonthAgoOrFirstMonthDate AND CL.FullDate)
	THEN 1 ELSE 0 END AS HasCVDB
 , CASE
    WHEN EXISTS (SELECT * 
	             FROM Reporting.dbo.FactCompanyStatuses CS 
	              JOIN dbo.DimDate DD ON CS.Date_key = DD.Date_key
				 WHERE CS.Company_key = CL.Company_key 
				  AND CS.HasActivatedLogo | CS.HasActivatedProfile = 1
				  AND DD.FullDate BETWEEN CL.FullDate1MonthAgoOrFirstMonthDate AND CL.FullDate)
	THEN 1 ELSE 0 END AS HasLogo
INTO #CompaniesLimitedList
FROM #СompaniesListByDates CL
WHERE EXISTS (SELECT * 
			  FROM Reporting.dbo.FactCompanyStatuses CS
			   JOIN Reporting.dbo.DimDate DD ON CS.Date_key = DD.Date_key
			  WHERE CS.Company_key = CL.Company_key 
			   AND DD.FullDate BETWEEN CL.FullDate1YearAgo AND DD.FullDate
			   AND HasPaidServices = 1);

-- вывод датасета для отчета

SELECT 
   Company_key
 , NotebookID
 , CompanyName
 , ManagerName
 , FullDate
 , Date_key
 , CompanyGroup
 , HasVacancy
 , HasHot
 , HasCVDB
 , HasLogo
 , CardColor
 , CASE 
   WHEN EXISTS (SELECT *
				FROM dbo.FactSpiderCompanyIndexes SCI WITH (NOLOCK)
				 JOIN dbo.DimSpiderCompany SC WITH (NOLOCK) ON SCI.SpiderCompanyID = SC.SpiderCompanyID
				WHERE SCI.Date_key = main.Date_key
				 AND SC.NotebookId = main.NotebookId
				 AND (SCI.IsBusiness = 1 OR SCI.IsPaidByTickets = 1 OR SCI.Paid = 1 OR HotVacancyCount &gt; 0 OR IsHasLogo = 1 OR IsHasLogo_OnMain = 1 OR VacancyCount &gt; 2)) THEN 1 ELSE 0 END AS IsPaidToWork
 , (SELECT TOP 1 UnqVacancyNum FROM #CompaniesLimitedList WHERE Company_key = main.Company_key ORDER BY Date_key DESC) AS UnqVacancyNum
 , (SELECT TOP 1 VacancyNum FROM #CompaniesLimitedList WHERE Company_key = main.Company_key ORDER BY Date_key DESC) AS VacancyNum
 , DENSE_RANK() OVER(ORDER BY CompanyName) AS RowNum
FROM #CompaniesLimitedList AS main
WHERE EXISTS (SELECT * FROM #CompaniesLimitedList WHERE Company_key = main.Company_Key AND CompanyGroup = @SelectedCompanyGroup AND FullDate = @SelectedDate)
ORDER BY Company_key, FullDate;</CommandText>
