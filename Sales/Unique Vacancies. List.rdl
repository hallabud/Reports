    <DataSet Name="DStDepartments">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SELECT Name 
FROM SRV16.RabotaUA2.dbo.Departments 
WHERE Name IN ('rabota.ua','Sales Force','Telesales','Teleinfo','Non-Resident')
ORDER BY Name</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DStManagers">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SELECT M.Name, D.Name AS DepartmentName
FROM SRV16.RabotaUA2.dbo.Manager M
 JOIN SRV16.RabotaUA2.dbo.Departments D ON M.DepartmentId = D.ID
WHERE D.Name IN ('rabota.ua','Sales Force','Telesales','Teleinfo','Non-Resident')
ORDER BY Name</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DepartmentName">
          <DataField>DepartmentName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!DepartmentName.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!Department.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
