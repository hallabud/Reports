  <DataSets>
    <DataSet Name="DstVacancyViews">
      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <CommandText>--SELECT 
--   dbo.fnDateByDay(VS.Day) AS FullDate
-- , VP.ID AS VacancyID
-- , VP.NotebookID
-- , 'http://rabota.ua/company' + CAST(VP.NotebookID AS VARCHAR) + '/vacancy' + CAST(VP.ID AS VARCHAR) AS VacancyURL
-- , VP.Name + ' (' + C.Name + ')' AS VacancyName 
-- , C.Name AS City
-- , NC.Name AS CompanyName
-- , VS.Detail AS ViewsNum
--FROM Analytics.dbo.VacancyPublished VP 
-- JOIN Analytics.dbo.MegaVIPVacancyStatistic VS ON VP.ID = VS.VacancyId AND dbo.fnDateByDay(VS.Day) &gt;= dbo.fnGetDatePart(VP.StateDate)
-- JOIN Analytics.dbo.NotebookCompany NC ON VP.NotebookID = NC.NotebookId
-- JOIN Analytics.dbo.VacancyCity VC ON VP.ID = VC.VacancyId
-- JOIN Analytics.dbo.City C ON VC.CityId = C.Id
--WHERE VP.ID IN (4896086,5724525,5650107,5663932,5694765)
--ORDER BY VP.ID, VS.Day 

WITH C AS
 (
SELECT *
 , ROW_NUMBER() OVER(PARTITION BY VSH.VacancyID ORDER BY VSH.DateTime DESC) AS RowNum
FROM Analytics.dbo.VacancyStateHistory VSH
WHERE VSH.VacancyId IN (5705459,5662538,5533201,5705757)
 AND VSH.State = 4
 )
SELECT 
   dbo.fnDateByDay(VS.Day) AS FullDate
 , C.VacancyId
 , NC.NotebookId
 , 'http://rabota.ua/company' + CAST(NC.NotebookID AS VARCHAR) + '/vacancy' + CAST(C.VacancyId AS VARCHAR) AS VacancyURL
 , V.Name + ' (' + CT.Name + ')' AS VacancyName 
 , CT.Name AS City
 , NC.Name AS CompanyName
 , VS.Detail AS ViewsNum
FROM C 
 JOIN Analytics.dbo.Vacancy V ON C.VacancyId = V.Id
 JOIN Analytics.dbo.NotebookCompany NC ON V.NotebookId = NC.NotebookId
 JOIN Analytics.dbo.VacancyCity VC ON V.Id = VC.VacancyId
 JOIN Analytics.dbo.City CT ON VC.CityId = CT.Id
 JOIN Analytics.dbo.MegaVIPVacancyStatistic VS 
	ON C.VacancyId = VS.VacancyId 
		AND dbo.fnDateByDay(VS.Day) &gt;= dbo.fnGetDatePart(C.DateTime)
		AND dbo.fnDateByDay(VS.Day) &lt;= 
		   CASE 
			 WHEN C.DateTime = C.DateTimeUpd 
			 THEN GETDATE() 
			 ELSE dbo.fnGetDatePart(C.DateTimeUpd) 
			END
WHERE RowNum = 1
ORDER BY C.VacancyID, VS.Day;</CommandText>
      </Query>
      <Fields>
        <Field Name="FullDate">
          <DataField>FullDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="VacancyID">
