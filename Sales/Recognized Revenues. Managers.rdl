  <DataSets>
    <DataSet Name="DStRecognizedRevenue">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
   DS.Service_key
 , DS.ServiceName
 , DS.ServiceGroup
 , DD.YearNum
 , 'Q' + CAST(DD.QuarterNum AS VARCHAR) AS QuarterNum
 , DD.MonthNum
 , DD.MonthNameRus
 , DD.MonthNameEng
 , DC.ManagerName
 , DC.DepartmentName
 , SUM(FRR.RecognizedRevenue) AS RecognizedRevenue
FROM Reporting.dbo.FactRecognizedRevenueNotebook FRR
 JOIN Reporting.dbo.DimDate DD ON FRR.Date_key = DD.Date_key
 JOIN Reporting.dbo.DimService DS ON FRR.Service_key = DS.Service_key
 JOIN Reporting.dbo.DimCompany DC ON FRR.NotebookID = DC.NotebookId
WHERE DD.FullDate BETWEEN @StartDate AND @EndDate
GROUP BY 
   DS.Service_key
 , DS.ServiceName
 , DS.ServiceGroup
 , DD.YearNum
 , DD.QuarterNum
 , DD.MonthNum
 , DD.MonthNameRus
 , DD.MonthNameEng
 , DC.ManagerName
 , DC.DepartmentName
ORDER BY Service_key, YearNum, MonthNum;</CommandText>
      </Query>
      <Fields>
        <Field Name="Service_key">
          <DataField>Service_key</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ServiceName">
          <DataField>ServiceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ServiceGroup">
