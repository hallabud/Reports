  <DataSets>
    <DataSet Name="DStMetaNotebooksList">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ManagerID">
            <Value>=Parameters!ManagerID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

DECLARE @Date_key INT = (SELECT MAX(Date_key) FROM Reporting.dbo.FactCompanyStatuses);
DECLARE @FullDate DATE = (SELECT FullDate FROM Reporting.dbo.DimDate WHERE Date_key = @Date_key);

-- Статус "привязки": 1 - Новая, Привязать к; 2 - Нет у нас такой компании; 3 - Непонятно к кому; 4 - Неинтересная компания
WITH C AS
 (
SELECT 
   MN.ID AS MetaNotebookID
 , MN.CompanyName AS MetaNotebookName
 , (SELECT COUNT(*) FROM SRV16.RabotaUA2.dbo.CRM_Action WHERE MetaNotebookID = MN.ID AND StateID = 2 AND TypeId &lt;&gt; 9) AS ActionsCountTotal
 , (SELECT TOP 1 CompleteDate FROM SRV16.RabotaUA2.dbo.CRM_Action WHERE MetaNotebookID = MN.ID  AND StateID = 2 AND TypeId &lt;&gt; 9 ORDER BY CompleteDate DESC) AS LastActionDate
 , SC.VacancyCount
 , CASE WHEN SC.IsPaidByTickets | SC.Paid | SC.IsBusiness | SC.IsHasLogo | SC.IsHasLogo_OnMain &gt; 0 THEN 1 ELSE 0 END AS IsPaidOnWork
 , M.Id AS ManagerID
FROM SRV16.RabotaUA2.dbo.SpiderCompany SC
 LEFT JOIN SRV16.RabotaUA2.dbo.Admin2_MetaNotebook MN ON SC.CompanyId = MN.WorkCompanyID
 LEFT JOIN SRV16.RabotaUA2.dbo.aspnet_Membership MMB ON MN.LoginEMail = MMB.Email
 LEFT JOIN SRV16.RabotaUA2.dbo.Manager M ON MMB.UserId = M.aspnet_UserUIN
 LEFT JOIN SRV16.RabotaUA2.dbo.Manager MTL ON M.TeamLead_ManagerID = MTL.Id
WHERE SC.Source = 1
 AND EXISTS (SELECT * FROM SRV16.RabotaUA2.dbo.Admin2_MetaNotebook WHERE WorkCompanyID = SC.CompanyID AND CreatorLoginEMail = 'irinash@rabota.ua') -- Мета-блокнот
 AND EXISTS (SELECT * FROM SRV16.RabotaUA2.dbo.Admin2_MetaNotebook WHERE WorkCompanyID = SC.CompanyID AND CreatorLoginEMail = 'irinash@rabota.ua') 
 AND SC.NotebookID IS NULL 
 AND NOT EXISTS (SELECT * FROM SRV16.RabotaUA2.dbo.SpiderCompany_LogByUser WHERE SpiderCompanyID = SC.Id AND Field = 'State' AND NewValue = 1 AND LoginEMail = 'irinash@rabota.ua')
 AND EXISTS (SELECT * FROM SRV16.RabotaUA2.dbo.SpiderCompany_LogByUser WHERE SpiderCompanyID = SC.Id AND Field= 'State' AND NewValue = 2)
 AND (EXISTS (SELECT * FROM SRV16.RabotaUA2.dbo.SpiderCompany_LogByUser WHERE SpiderCompanyID = SC.Id AND Field = 'State' AND NewValue IN (1,4) AND LoginEMail = 'irinash@rabota.ua') 
		OR EXISTS (SELECT * FROM SRV16.RabotaUA2.dbo.Admin2_MetaNotebook WHERE WorkCompanyID = SC.CompanyID AND CreatorLoginEMail = 'irinash@rabota.ua'))
 AND M.Id IN (@ManagerID) 
 )
SELECT * 
FROM C ;</CommandText>
      </Query>
