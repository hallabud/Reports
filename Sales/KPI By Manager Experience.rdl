    <DataSet Name="DStRecognizedRevenue">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

DECLARE @TodayDate DATE = CONVERT(DATE, GETDATE());
DECLARE @ReportDate DATE = (SELECT FirstMonthDate FROM dbo.DimDate WHERE FullDate = @TodayDate);
DECLARE @StartDate DATE = (SELECT FirstMonthDate FROM dbo.DimDate WHERE FullDate = DATEADD(MONTH, -1, @ReportDate));
DECLARE @EndDate DATE = (SELECT LastMonthDate FROM dbo.DimDate WHERE FullDate = DATEADD(MONTH, -1, @ReportDate));
DECLARE @StartDateKey INT = (SELECT Date_key FROM dbo.DimDate WHERE FullDate = @StartDate);
DECLARE @EndDateKey INT = (SELECT Date_key FROM dbo.DimDate WHERE FullDate = @EndDate);

SELECT 
   M.Name AS ManagerName
 , CASE
	WHEN M.AddDate &gt;= DATEADD(MONTH, -6, @TodayDate) THEN '0 - 6 months'
	WHEN M.AddDate &gt;= DATEADD(MONTH, -12, @TodayDate) THEN '6 - 12 months'
	WHEN M.AddDate &gt;= DATEADD(MONTH, -18, @TodayDate) THEN '12 - 18 months'
	WHEN M.AddDate &gt;= DATEADD(MONTH, -24, @TodayDate) THEN '18 - 24 months'
	WHEN M.AddDate &gt;= DATEADD(YEAR, -5, @TodayDate) THEN '2 - 5 years'
	WHEN M.AddDate &lt; DATEADD(YEAR, -5, @TodayDate) THEN '5 years +'
   END AS ManagerAgeGroup
 , CASE
	WHEN M.AddDate &gt;= DATEADD(MONTH, -6, @TodayDate) THEN 1
	WHEN M.AddDate &gt;= DATEADD(MONTH, -12, @TodayDate) THEN 2
	WHEN M.AddDate &gt;= DATEADD(MONTH, -18, @TodayDate) THEN 3
	WHEN M.AddDate &gt;= DATEADD(MONTH, -24, @TodayDate) THEN 4
	WHEN M.AddDate &gt;= DATEADD(YEAR, -5, @TodayDate) THEN 5
	WHEN M.AddDate &lt; DATEADD(YEAR, -5, @TodayDate) THEN 6
   END AS ManagerAgeGroupOrderNum
 , SUM(RecognizedRevenue) SumRR
FROM Reporting.dbo.FactRecognizedRevenueNotebook RRN
 JOIN SRV16.RabotaUA2.dbo.NotebookCompany NC ON RRN.NotebookID = NC.NotebookId
 JOIN SRV16.RabotaUA2.dbo.Manager M ON NC.ManagerId = M.Id
WHERE Date_key BETWEEN @StartDateKey AND @EndDateKey
 AND M.DepartmentID = 2 AND IsTatarovaGroup = 0 AND IsLoyaltyGroup = 0 AND IsForTesting = 0 AND IsReportExcluding = 0
GROUP BY
   M.Name
 , CASE
	WHEN M.AddDate &gt;= DATEADD(MONTH, -6, @TodayDate) THEN '0 - 6 months'
	WHEN M.AddDate &gt;= DATEADD(MONTH, -12, @TodayDate) THEN '6 - 12 months'
	WHEN M.AddDate &gt;= DATEADD(MONTH, -18, @TodayDate) THEN '12 - 18 months'
	WHEN M.AddDate &gt;= DATEADD(MONTH, -24, @TodayDate) THEN '18 - 24 months'
	WHEN M.AddDate &gt;= DATEADD(YEAR, -5, @TodayDate) THEN '2 - 5 years'
	WHEN M.AddDate &lt; DATEADD(YEAR, -5, @TodayDate) THEN '5 years +'
   END
 , CASE
	WHEN M.AddDate &gt;= DATEADD(MONTH, -6, @TodayDate) THEN 1
	WHEN M.AddDate &gt;= DATEADD(MONTH, -12, @TodayDate) THEN 2
	WHEN M.AddDate &gt;= DATEADD(MONTH, -18, @TodayDate) THEN 3
	WHEN M.AddDate &gt;= DATEADD(MONTH, -24, @TodayDate) THEN 4
	WHEN M.AddDate &gt;= DATEADD(YEAR, -5, @TodayDate) THEN 5
	WHEN M.AddDate &lt; DATEADD(YEAR, -5, @TodayDate) THEN 6
   END</CommandText>
      </Query>
      <Fields>
        <Field Name="ManagerName">
          <DataField>ManagerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ManagerAgeGroup">
          <DataField>ManagerAgeGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ManagerAgeGroupOrderNum">
          <DataField>ManagerAgeGroupOrderNum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SumRR">
          <DataField>SumRR</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
