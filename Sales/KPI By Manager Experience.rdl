	WHEN M.AddDate &lt; DATEADD(YEAR, -5, @TodayDate) THEN '5 years +'
   END
 , CASE
	WHEN M.AddDate &gt;= DATEADD(MONTH, -6, @TodayDate) THEN 1
	WHEN M.AddDate &gt;= DATEADD(MONTH, -12, @TodayDate) THEN 2
	WHEN M.AddDate &gt;= DATEADD(MONTH, -18, @TodayDate) THEN 3
	WHEN M.AddDate &gt;= DATEADD(MONTH, -24, @TodayDate) THEN 4
	WHEN M.AddDate &gt;= DATEADD(YEAR, -5, @TodayDate) THEN 5
	WHEN M.AddDate &lt; DATEADD(YEAR, -5, @TodayDate) THEN 6
   END;</CommandText>
