    <DataSet Name="DSt_Team">
      <Query>
        <DataSourceName>Reporting</DataSourceName>
        <CommandText> select distinct
 m.TeamLead_ManagerID, m1.name as SM

 from 
 SRV16.RabotaUA2.dbo.Manager M with(nolock)
 join SRV16.RabotaUA2.dbo.Manager M1 with(nolock) on m1.id = m.TeamLead_ManagerID
 where m.departmentid = 12
 </CommandText>
      </Query>
      <Fields>
        <Field Name="TeamLead_ManagerID">
          <DataField>TeamLead_ManagerID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SM">
          <DataField>SM</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DSt_STM">
      <Query>
        <DataSourceName>Reporting</DataSourceName>
        <CommandText> select distinct
 m.TeamLead_ManagerID as SM, m.STM_ManagerID, m1.name as STM
 from 
 SRV16.RabotaUA2.dbo.Manager M with(nolock)
 join SRV16.RabotaUA2.dbo.Manager M1 with(nolock) on m1.id = m.STM_ManagerID
 where m.departmentid = 12
 and m.STM_ManagerID &lt;&gt; m.TeamLead_ManagerID
 order by m1.name</CommandText>
      </Query>
      <Fields>
        <Field Name="SM">
          <DataField>SM</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="STM_ManagerID">
          <DataField>STM_ManagerID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="STM">
          <DataField>STM</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!SM.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!Team.Value()</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="DSt_Manager">
      <Query>
        <DataSourceName>Reporting</DataSourceName>
        <CommandText>  select 
		Id ManagerID,
		name ManagerName,
		case when m.id = 212 then 212 else 
			case when EXISTS (SELECT * FROM SRV16.RabotaUA2.dbo.Manager with(nolock) WHERE STM_ManagerID = M.Id AND DepartmentID IN (2,12)) OR (STM_ManagerID = TeamLead_ManagerID AND StatusInTransactionGroup IS NULL) then m.id else 
				coalesce(M.STM_ManagerID, M.Id) 
			end
		end STMid,
		case when m.id = 212 then 1 else 
		case when coalesce(M.STM_ManagerID, M.Id) = coalesce(TeamLead_ManagerID, Id) and M.StatusInTransactionGroup is null and coalesce(TeamLead_ManagerID, Id) &lt;&gt; M.Id 
			and exists (select * from SRV16.RabotaUA2.dbo.Manager with(nolock) where STM_ManagerID = M.Id) then 1 else 0 end 
		end IsSTM,
		case when m.id = 212 then 212 else coalesce(TeamLead_ManagerID, Id) end SMid

	from SRV16.RabotaUA2.dbo.Manager M with(nolock)  join SRV16.RabotaUA2.dbo.aspnet_Membership MMB with(nolock) ON M.aspnet_UserUIN = MMB.UserId
	where ((IsForTesting = 0 and IsReportExcluding = 0 and (TeamLead_ManagerID is not null or exists (select * from SRV16.RabotaUA2.dbo.Manager with(nolock) where TeamLead_ManagerID = M.Id))) or m.id in (1, 212))
	and m.departmentid = 12
	and TeamLead_ManagerID &lt;&gt; id</CommandText>
      </Query>
      <Fields>
        <Field Name="ManagerID">
          <DataField>ManagerID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ManagerName">
          <DataField>ManagerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="STMid">
          <DataField>STMid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="IsSTM">
          <DataField>IsSTM</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SMid">
          <DataField>SMid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!STMid.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!STM.Value()</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
