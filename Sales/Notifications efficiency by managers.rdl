  <DataSets>
    <DataSet Name="DStTramsactionEfficiencyByAlertTypeByPeriod">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>	--DECLARE @StartDate DATETIME = '20180502'
	--DECLARE @ENdDate DATETIME = '20180503'

	-- менеджеры "отдела транзакционных продаж"
	IF OBJECT_ID('tempdb..#Managers','U') IS NOT NULL DROP TABLE #Managers;

		SELECT Reporting.dbo.udf_FullMonthsSeparation(AddDate, @EndDate) AS ManagerAgeFullMonth
			, M.Id AS ManagerID
			, M.Name AS ManagerName
			, MMB.Email AS ManagerLoginEmail
			,m.DepartmentGroup
		INTO #Managers
		FROM SRV16.RabotaUA2.dbo.Manager M
			JOIN SRV16.RabotaUA2.dbo.aspnet_Membership MMB ON M.aspnet_UserUIN = MMB.UserId
		WHERE DepartmentID = 12;

	-- выгребаем с линкед-сервера все выполненные действия во времянку. потом работаем с ней.
	IF OBJECT_ID('tempdb..#CompletedActions','U') IS NOT NULL DROP TABLE #CompletedActions;  

		SELECT *
		INTO #CompletedActions
		FROM SRV16.RabotaUA2.dbo.CRM_Action
		WHERE StateID IN (2) AND TypeID NOT IN (6,8,9)
		 AND CompleteDate &gt;= @StartDate
		 UNION ALL
		 SELECT *
		FROM SRV16.RabotaUA2.dbo.CRM_Action
		WHERE StateID IN (1) AND TypeID NOT IN (6,8,9)
		 ;

	IF OBJECT_ID('tempdb..#Alarms','U') IS NOT NULL DROP TABLE #Alarms; 
	-- выгребаем с линкед-сервера все алармы во времянку. потом работаем с ней.
		SELECT ALRM.*, ATP.Name AS AlarmTypeName, ATP.IsPayment, M.ManagerAgeFullMonth, M.ManagerLoginEmail, M.ManagerName, m.DepartmentGroup
		INTO #Alarms
		FROM SRV16.RabotaUA2.dbo.CRM_Alarm ALRM
		 JOIN #Managers M ON ALRM.ManagerID = M.ManagerID
		 JOIN SRV16.RabotaUA2.dbo.CRM_AlarmType ATP ON ALRM.AlarmTypeID = ATP.ID 
		WHERE (COALESCE(ALRM.SentDate, ALRM.AddDate) BETWEEN @StartDate AND @EndDate+1 AND ALRM.AlarmTypeID IN (67,68,10,63,59,58,69,70,71,72,73,5,6,3,31) AND isdeleted = 0)
	       OR (COALESCE(ALRM.SentDate, ALRM.AddDate) BETWEEN @StartDate AND @EndDate+1 AND ALRM.AlarmTypeID IN (5,6)); 
		   --список алертов в гугл-докс https://docs.google.com/spreadsheets/d/1-UDl-AACNPYPS7mHUF5ZaFTW8r-sWBtunl-4lhSXrvE/edit?ts=5ad9d516

	-- уведомления, отправленные менеджерам-транзакционщикам за период. 
	-- к уведомлениям, отправленным за период, привязываем действия, сделки и оплаты и т.д.
	-- отправная точка - уведомление (транзакция). 
	-- если например в этот период была оплата, но по уведомлению пришедшему в другой период, то ее тут не будет
	-- так правильно, если кому-то не нравится, я популярно объъясню
	
	IF OBJECT_ID('tempdb..#AlarmsActionsDeals','U') IS NOT NULL DROP TABLE #AlarmsActionsDeals;
	
		SELECT 
			  ALRM.ManagerID
			, ALRM.ManagerLoginEmail
			, ALRM.ManagerName
			, alrm.DepartmentGroup
			, ALRM.ManagerAgeFullMonth
			, ALRM.AlarmTypeID
			, COALESCE(ALRM.SentDate, ALRM.AddDate) AS AlarmDate
			, ALRM.AlarmTypeName
			/*
			, CASE 
				WHEN ALRM.AlarmTypeID = 3 
					THEN 
					   (SELECT TOP 1 ATP2.Name 
						FROM SRV16.RabotaUA2.dbo.CRM_Alarm ALRM2
						 JOIN SRV16.RabotaUA2.dbo.CRM_AlarmType ATP2 ON ALRM2.AlarmTypeID = ATP2.ID
						 JOIN SRV16.RabotaUA2.dbo.CRM_Action ACT2 ON ALRM2.ActionID = ACT2.ID
						WHERE ALRM2.NotebookID = ALRM.NotebookID
						 AND ATP2.IsPayment = 0 
						 AND ATP2.ID &lt;&gt; 3
						 AND ALRM2.ID &lt; ALRM.ID
						 AND ATP2.IsHidden = 0
						 and isdeleted = 0
						ORDER BY ACT2.CompleteDate DESC) -- уведомление, по которому было действие и которое было до текущего уведомления
				END AS AlarmType2
			
			, CASE 
				WHEN ALRM.AlarmTypeID = 3 
					THEN 
					   (SELECT TOP 1 ACT2.CompleteDate
						FROM SRV16.RabotaUA2.dbo.CRM_Alarm ALRM2
						 JOIN SRV16.RabotaUA2.dbo.CRM_AlarmType ATP2 ON ALRM2.AlarmTypeID = ATP2.ID
						 JOIN SRV16.RabotaUA2.dbo.CRM_Action ACT2 ON ALRM2.ActionID = ACT2.ID
						WHERE ALRM2.NotebookID = ALRM.NotebookID
						 AND ATP2.IsPayment = 0 
						 AND ATP2.ID &lt;&gt; 3
						 AND ALRM2.ID &lt; ALRM.ID
						 AND ATP2.IsHidden = 0
						 and isdeleted = 0
						ORDER BY ACT2.CompleteDate DESC) -- дата уведомления, по которому было действие и которое было до текущего уведомления
				END AS AlarmDate2
*/
			, CASE ALRM.IsPayment WHEN 1 THEN 'Сервисные уведомления' ELSE 'Стандартные уведомления' END AS AlarmCategory
			, ALRM.ID AS AlarmID
			, ALRM.NotebookID
			, ALRM.AccYear
			, ALRM.AccID
			--, A.ID AS ActionID
			, CASE WHEN StateID = 1 THEN A.ID END AS PlanActionID 
			, CASE WHEN StateID = 2 THEN A.ID END AS ActionID
			, COALESCE(ALRM.DealID, A.DealID) AS DealID
		INTO #AlarmsActionsDeals
		FROM #Alarms ALRM
			LEFT JOIN #CompletedActions A ON ALRM.ActionID = A.ID
		 ;
		 
		 SELECT 
		    AAD.ManagerID
		  , AAD.ManagerName
		  , AAD.ManagerLoginEmail
		  , aad.DepartmentGroup
		  , AAD.ManagerAgeFullMonth
		  , AAD.AlarmID
		  , AAD.AlarmTypeID
		  , AAD.AlarmTypeName AS AlarmTypeName_Initial
		  --, CASE WHEN AlarmDate2 &lt; DATEADD(DAY, -30, AlarmDate) THEN AAD.AlarmTypeName ELSE COALESCE(AAD.AlarmType2, AAD.AlarmTypeName) END AS AlarmTypeName
		  , AAD.AlarmTypeName
		  , AAD.AlarmCategory
		  , AAD.NotebookID
		  , AAD.ActionID
		  , AAD.PlanActionID
		  --, COALESCE(AAD.DealID, O2.DealID) AS DealID
		  , AAD.DealID
		  , OA.ID OrderID
		  , OAP.ID OrderPaymentID
		  --, CASE WHEN OAP.PaySum &gt; 0 THEN COALESCE(AAD.DealID, O2.DealID) END AS PaidDealID
		  , CASE WHEN OAP.PaySum &gt; 0 THEN AAD.DealID END AS PaidDealID
		  , SUM(PaySum) AS PaySum
		 FROM #AlarmsActionsDeals AAD 
		  LEFT JOIN SRV16.RabotaUA2.dbo.Orders O ON AAD.DealID = O.DealID AND O.IsDealMain = 1
		  LEFT JOIN SRV16.RabotaUA2.dbo.Order_Acc OA ON O.ID = OA.OrderID
		  LEFT JOIN SRV16.RabotaUA2.dbo.Order_AccPayment OAP ON COALESCE(AAD.AccYear, OA.Year) = OAP.AccYear AND COALESCE(AAD.AccID, OA.ID) = OAP.AccID
		  --LEFT JOIN SRV16.RabotaUA2.dbo.Order_Acc OA2 ON COALESCE(AAD.AccYear, OA.Year) = OA2.Year AND COALESCE(AAD.AccID, OA.ID) = OA2.ID
		  --LEFT JOIN SRV16.RabotaUA2.dbo.Orders O2 ON OA2.OrderID = O2.ID
		 GROUP BY
		    AAD.ManagerID
		  , AAD.ManagerName
		  , AAD.ManagerLoginEmail
		  , aad.DepartmentGroup
		  , AAD.ManagerAgeFullMonth
		  , AAD.AlarmID
		  , AAD.AlarmTypeID
		  --, CASE WHEN AlarmDate2 &lt; DATEADD(DAY, -30, AlarmDate) THEN AAD.AlarmTypeName ELSE COALESCE(AAD.AlarmType2, AAD.AlarmTypeName) END
		  , AAD.AlarmTypeName
		  , AAD.AlarmCategory
		  , AAD.NotebookID
		  , AAD.ActionID
		  , AAD.PlanActionID
		  /*, COALESCE(AAD.DealID, O2.DealID)
		  , CASE WHEN OAP.PaySum &gt; 0 THEN COALESCE(AAD.DealID, O2.DealID) END*/
		  , AAD.DealID
		  , OA.ID
		  , OAP.ID
		  , CASE WHEN OAP.PaySum &gt; 0 THEN AAD.DealID END
		  ;


--имя менеджера	| количество уведомлений | количество уникальных компаний, по которым поступило уведомление	количество действий менеджера	
--конверсия обработки уведомлений,% | из них позитивных	| доля позитивных действий, % | количество сделок | количество платных сделок | 
--конверсия: платные сделки,% | сумма оплат	| средний чек</CommandText>
      </Query>
