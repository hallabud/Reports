  <Code>Function SecondsToHoursMinutesSeconds(ByVal vSecs As Integer) As String
 
Dim Seconds AS Integer
Dim Mins As Integer
Dim Hours As Integer
 
vSecs = IIF(vSecs &lt;= 0, 0, vSecs)
 
Hours = Floor(vSecs / 3600)
Mins = Floor(vSecs / 60) - Hours * 60
Seconds = vSecs - Hours * 3600 - Mins * 60
 
SecondsToHoursMinutesSeconds = IIF(Hours &lt;= 9, "0" &amp; CStr(Hours), CStr(Hours)) &amp; ":" &amp; IIF(Mins &lt;= 9, "0" &amp; CStr(Mins), CStr(Mins)) &amp; ":" &amp; IIF(Seconds &lt;= 9, "0" &amp; CStr(Seconds), CStr(Seconds))
 
Return SecondsToHoursMinutesSeconds
 
End Function</Code>
