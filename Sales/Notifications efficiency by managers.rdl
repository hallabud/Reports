    <DataSet Name="DStTransactionSTM">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>	SELECT 
		M.Id AS ManagerID,
		M.Name AS ManagerName,
		
		case 
		when m.id = 212 
		then 212 
		else 
				case when EXISTS (SELECT * 
								  FROM SRV16.RabotaUA2.dbo.Manager 
								  WHERE STM_ManagerID = M.Id AND DepartmentID IN (2,12)) OR (STM_ManagerID = TeamLead_ManagerID) 
		        then m.id 
				else coalesce(M.STM_ManagerID, M.Id) 
				end
		end STMid
		 ,MMB.Email AS ManagerLoginEmail
	INTO #managers
	FROM SRV16.RabotaUA2.dbo.Manager M
		 JOIN SRV16.RabotaUA2.dbo.aspnet_Membership MMB ON M.aspnet_UserUIN = MMB.UserId
	WHERE DepartmentID = 12

	select distinct
		m.stmid,
		mm.name

	from #managers m left join srv16.rabotaua2.dbo.manager mm on m.stmid = mm.id</CommandText>
      </Query>
      <Fields>
        <Field Name="stmid">
          <DataField>stmid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
