    <DataSet Name="DStTransactionManagers">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>	SELECT 
		M.Id AS ManagerID,
		M.Name AS ManagerName,
		
		case 
		when m.id = 212 
		then 212 
		else 
				case when EXISTS (SELECT * 
								  FROM SRV16.RabotaUA2.dbo.Manager 
								  WHERE STM_ManagerID = M.Id AND DepartmentID IN (2,12)) OR (STM_ManagerID = TeamLead_ManagerID) 
		        then m.id 
				else coalesce(M.STM_ManagerID, M.Id) 
				end
