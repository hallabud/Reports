    <DataSet Name="DStManagers">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SELECT LOWER(AM.Email) AS Email, Name, 'RABOTA\' + LEFT(AM.EMail, CHARINDEX('@', AM.Email) - 1) AS UserID
FROM SRV16.RabotaUA2.dbo.Manager M
 JOIN SRV16.RabotaUA2.dbo.aspnet_Membership AM ON M.aspnet_UserUIN = AM.UserID
WHERE DepartmentID IN (1,2,3,4,10,12) 
 AND IsForTesting = 0 
ORDER BY Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Email">
          <DataField>Email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UserID">
          <DataField>UserID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
