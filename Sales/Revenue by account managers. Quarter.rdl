  <DataSources>
    <DataSource Name="DSrcReporting">
      <DataSourceReference>DSrcReporting</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>20cf8a9a-4d2c-49fa-8b46-e3a1ca54b0d1</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DStMain">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Managers">
            <Value>=Parameters!Managers.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DD.YearNum, DD.QuarterNum, DD.MonthNameRus, DD.MonthNum, M.Name AS Manager, SUM(RecognizedRevenue) AS SumRecognizedRevenue
FROM Reporting.dbo.FactRecognizedRevenueNotebook RRN
 JOIN Reporting.dbo.DimDate DD ON RRN.Date_key = DD.Date_key
 JOIN Analytics.dbo.NotebookCompany NC ON RRN.NotebookID = NC.NotebookID
 JOIN Analytics.dbo.Manager M ON NC.ManagerID = M.ID
WHERE M.DepartmentID = 2
 AND DD.FullDate BETWEEN @StartDate AND @EndDate
 AND M.ID IN (@Managers)
GROUP BY DD.YearNum, DD.QuarterNum, DD.MonthNameRus, DD.MonthNum, M.Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="YearNum">
          <DataField>YearNum</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="QuarterNum">
          <DataField>QuarterNum</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="MonthNameRus">
          <DataField>MonthNameRus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MonthNum">
          <DataField>MonthNum</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="Manager">
          <DataField>Manager</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SumRecognizedRevenue">
          <DataField>SumRecognizedRevenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DStManagers">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SELECT ID, Name FROM SRV12.RabotaUA2.dbo.Manager WHERE DepartmentID = 2 AND IsForTesting = 0 ORDER BY Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
