    <DataSet Name="DStClientsMonths">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ManagerID">
            <Value>=Parameters!ManagerID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

IF OBJECT_ID('tempdb..#RevenueMonth','U') IS NOT NULL DROP TABLE #RevenueMonth;
IF OBJECT_ID('tempdb..#RevenueMonthActivity','U') IS NOT NULL DROP TABLE #RevenueMonthActivity;
IF OBJECT_ID('tempdb..#Companies') IS NOT NULL DROP TABLE #Companies;

-- потребление по компаниям помесячно за интересующий период
  SELECT
     M.Id AS ManagerID
   , M.Name AS ManagerName
   , NC.NotebookId
   , NC.Name AS CompanyName
   , DD.YearNum
   , DD.MonthNum
   , DD.MonthNameEng
   , DD.MonthNameRus
   , RRN.Service_key
   , SUM(RRN.RecognizedRevenue) AS SumRecognizedRevenue
  INTO #RevenueMonth
  FROM SRV16.RabotaUA2.dbo.NotebookCompany AS NC 
   INNER JOIN SRV16.RabotaUA2.dbo.Manager AS M ON NC.ManagerId = M.Id 
   INNER JOIN Reporting.dbo.FactRecognizedRevenueNotebook AS RRN ON NC.NotebookId = RRN.NotebookID
   INNER JOIN Reporting.dbo.DimDate AS DD ON RRN.Date_key = DD.Date_key
  WHERE M.Id IN (@ManagerID) 
   AND DD.FullDate BETWEEN @StartDate AND @EndDate
  GROUP BY 
     M.Id
   , M.Name
   , NC.NotebookId
   , NC.Name
   , DD.YearNum
   , DD.MonthNum
   , DD.MonthNameEng
   , DD.MonthNameRus
   , RRN.Service_key
  HAVING SUM(RRN.RecognizedRevenue) &gt; 0;


-- потребление + группа активности
  SELECT 
	   ManagerID
	 , ManagerName
	 ,  CASE 
		 WHEN EXISTS (SELECT * 
					  FROM #RevenueMonth
					  WHERE NotebookId = RM.NotebookId 
					   AND YearNum = RM.YearNum
					   AND MonthNum = RM.MonthNum
					   AND Service_key &lt;&gt; 10
					   AND SumRecognizedRevenue &gt; 0) 
		 THEN 'С доходом: Активный клиент' ELSE 'С доходом: Пассивный клиент' END AS ClientType
	 , YearNum
	 , MonthNum
	 , MonthNameEng
	 , MonthNameRus
	 , NotebookId
	 , Service_key
  INTO #RevenueMonthActivity
  FROM #RevenueMonth AS RM;

-- суммы потраченных публикаций компаниями помесячно 
 SELECT
    M.Id AS ManagerID
  , M.Name AS ManagerName
  , DD.YearNum
  , DD.MonthNum
  , DD.MonthNameEng
  , DD.MonthNameRus
  , NC.NotebookId
  , SUM(NCS.SpendCount) AS PubNum
 INTO #Companies
 FROM SRV16.RabotaUA2.dbo.NotebookCompany AS NC 
  INNER JOIN SRV16.RabotaUA2.dbo.Manager AS M ON NC.ManagerId = M.Id 
  CROSS JOIN Reporting.dbo.DimDate AS DD
  LEFT JOIN SRV16.RabotaUA2.dbo.NotebookCompany_Spent NCS ON NC.NotebookID = NCS.NotebookID AND NCS.AddDate BETWEEN DD.FirstMonthDate AND DD.LastMonthDate + 1
 WHERE DD.FullDate BETWEEN @StartDate AND @EndDate 
  AND DD.DayNum = 1 
  AND M.Id IN (@ManagerID)
  AND NOT EXISTS (SELECT *
                  FROM SRV16.RabotaUA2.dbo.NotebookCompanyMerged AS NCM
                  WHERE NCM.SourceNotebookID = NC.NotebookId) 
  AND EXISTS (SELECT *
              FROM SRV16.RabotaUA2.dbo.Notebook AS N
              WHERE Id = NC.NotebookId 
			   AND NotebookStateId IN (5, 7)) 
  AND NOT EXISTS (SELECT *
				  FROM #RevenueMonth
                  WHERE NotebookId = NC.NotebookId AND YearNum = DD.YearNum AND MonthNum = DD.MonthNum)
 GROUP BY 
	M.Id
  , M.Name
  , DD.YearNum
  , DD.MonthNum
  , DD.MonthNameEng
  , DD.MonthNameRus
  , NC.NotebookId;


SELECT
   ManagerID
 , ManagerName
 , CASE WHEN PubNum IS NULL THEN 'Нет потраченных публикаций' WHEN PubNum = 0 THEN 'Нет потраченных публикаций' ELSE 'Есть потраченные публикации' END AS ClientType
 , YearNum
 , MonthNum
 , MonthNameEng
 , MonthNameRus
 , COUNT(DISTINCT NotebookId) AS ClientNum
FROM #Companies
GROUP BY 
   ManagerID
 , ManagerName
 , CASE WHEN PubNum IS NULL THEN 'Нет потраченных публикаций' WHEN PubNum = 0 THEN 'Нет потраченных публикаций' ELSE 'Есть потраченные публикации' END
 , YearNum
 , MonthNum
 , MonthNameEng
 , MonthNameRus

UNION ALL

SELECT 
   ManagerID
 , ManagerName
 , ClientType
 , YearNum
 , MonthNum
 , MonthNameEng
 , MonthNameRus
 , COUNT(DISTINCT NotebookId) AS ClientNum
FROM #RevenueMonthActivity
GROUP BY 
   ManagerID
 , ManagerName
 , ClientType
 , YearNum
 , MonthNum
 , MonthNameEng
 , MonthNameRus;</CommandText>
