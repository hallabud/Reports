        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

IF OBJECT_ID('tempdb..#RevenueQuarter','U') IS NOT NULL DROP TABLE #RevenueQuarter;
IF OBJECT_ID('tempdb..#RevenueQuarterActivity','U') IS NOT NULL DROP TABLE #RevenueQuarterActivity;
IF OBJECT_ID('tempdb..#Companies','U') IS NOT NULL DROP TABLE #Companies;

  -- потребление поквартально
  SELECT
    M.Id AS ManagerID
   , M.Name AS ManagerName
   , NC.NotebookId
   , NC.Name AS CompanyName
   , DD.YearNum
   , DD.QuarterNum
   , RRN.Service_key
   , SUM(RRN.RecognizedRevenue) AS SumRecognizedRevenue
  INTO #RevenueQuarter
  FROM SRV16.RabotaUA2.dbo.NotebookCompany AS NC 
   INNER JOIN SRV16.RabotaUA2.dbo.Manager AS M ON NC.ManagerId = M.Id 
   INNER JOIN Reporting.dbo.FactRecognizedRevenueNotebook AS RRN ON NC.NotebookId = RRN.NotebookID
   INNER JOIN Reporting.dbo.DimDate AS DD ON RRN.Date_key = DD.Date_key
  WHERE M.Id IN (@ManagerID) 
   AND DD.FullDate BETWEEN @StartDate AND @EndDate
  GROUP BY 
     M.Id
   , M.Name
   , NC.NotebookId
   , NC.Name
   , DD.YearNum
   , DD.QuarterNum
   , RRN.Service_key
  HAVING SUM(RRN.RecognizedRevenue) &gt; 0

  -- потребление поквартально с группами активности
  SELECT 
   ManagerID
 , ManagerName
 ,  CASE 
     WHEN EXISTS (SELECT * 
                  FROM #RevenueQuarter
                  WHERE NotebookId = RQ.NotebookId 
				   AND YearNum = RQ.YearNum
				   AND QuarterNum = RQ.QuarterNum
				   AND Service_key &lt;&gt; 10
				   AND SumRecognizedRevenue &gt; 0) 
     THEN 'С доходом: Активный клиент' ELSE 'С доходом: Пассивный клиент' END AS ClientType
 , YearNum
 , QuarterNum
 , NotebookId
 , Service_key
INTO #RevenueQuarterActivity
FROM #RevenueQuarter AS RQ

 -- траты публикаций компаниями
 SELECT
    M.Id AS ManagerID
  , M.Name AS ManagerName
  , DD.YearNum
  , DD.QuarterNum
  , NC.NotebookId
  , SUM(SpendCount) AS PubNum
 INTO #Companies
 FROM SRV16.RabotaUA2.dbo.NotebookCompany AS NC 
  INNER JOIN SRV16.RabotaUA2.dbo.Manager AS M ON NC.ManagerId = M.Id 
  CROSS JOIN Reporting.dbo.DimDate AS DD
  LEFT JOIN SRV16.RabotaUA2.dbo.NotebookCompany_Spent NCS ON NC.NotebookID = NCS.NotebookId AND NCS.AddDate BETWEEN  DD.FirstQuarterDate AND DD.LastQuarterDate + 1
 WHERE DD.FullDate BETWEEN @StartDate AND @EndDate 
  AND DD.DayNum = 1 
  AND M.Id IN (@ManagerID) 
  AND NOT EXISTS (SELECT *
                  FROM SRV16.RabotaUA2.dbo.NotebookCompanyMerged AS NCM
                  WHERE NCM.SourceNotebookID = NC.NotebookId) 
  AND EXISTS (SELECT *
              FROM SRV16.RabotaUA2.dbo.Notebook AS N
              WHERE Id = NC.NotebookId 
			   AND NotebookStateId IN (5, 7)) 
  AND NOT EXISTS (SELECT *
				  FROM #RevenueQuarter
                  WHERE NotebookId = NC.NotebookId AND YearNum = DD.YearNum AND QuarterNum = DD.QuarterNum)
GROUP BY 
	M.Id
  , M.Name
  , DD.YearNum
  , DD.QuarterNum
  , NC.NotebookId;


SELECT
   ManagerID
 , ManagerName
 , CASE WHEN PubNum IS NULL THEN 'Нет потраченных публикаций' WHEN PubNum = 0 THEN 'Нет потраченных публикаций' ELSE 'Есть потраченные публикации' END AS ClientType
 , YearNum
 , QuarterNum
 , COUNT(DISTINCT NotebookId) AS ClientNum
FROM #Companies
GROUP BY 
   ManagerID
 , ManagerName
 , CASE WHEN PubNum IS NULL THEN 'Нет потраченных публикаций' WHEN PubNum = 0 THEN 'Нет потраченных публикаций' ELSE 'Есть потраченные публикации' END
 , YearNum
 , QuarterNum

UNION ALL

SELECT 
   ManagerID
 , ManagerName
 , ClientType
 , YearNum
 , QuarterNum
 , COUNT(DISTINCT NotebookId) AS ClientNum
FROM #RevenueQuarterActivity
GROUP BY 
   ManagerID
 , ManagerName
 , ClientType
 , YearNum
 , QuarterNum;</CommandText>
