  <DataSets>
    <DataSet Name="DstCvdbWeeklyLimit">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

DECLARE @StartDate DATETIME = Analytics.dbo.fnGetDatePart(GETDATE() - 1);
DECLARE @EndDate DATETIME = Analytics.dbo.fnGetDatePart(GETDATE() - 1);

SELECT DISTINCT DVR.EmployerNotebookID, NC.Name AS NotebookName, M.Name AS Manager, MU.Name AS MultiUserName, MU.Phone, MU.EMail
FROM SRV16.RabotaUA2.dbo.DailyViewedResume DVR WITH (NOLOCK)
 JOIN SRV16.RabotaUA2.dbo.NotebookCompany NC WITH (NOLOCK) ON DVR.EmployerNotebookID = NC.NotebookID
 JOIN SRV16.RabotaUA2.dbo.Manager M WITH (NOLOCK) ON NC.ManagerID = M.ID
 JOIN (SELECT NotebookID, MultiUserID FROM SRV16.RabotaUA2.dbo.NotebookCompanyResumeView NCRV WITH (NOLOCK) WHERE NCRV.AddDate BETWEEN DATEADD(DAY, -6, @EndDate) AND @EndDate) RV ON DVR.EmployerNotebookID = RV.NotebookID
 JOIN SRV16.RabotaUA2.dbo.MultiUser MU ON RV.MultiUserID = MU.ID
WHERE ViewDate BETWEEN @StartDate AND @EndDate
	AND NOT EXISTS (SELECT *
					FROM SRV16.RabotaUA2.dbo.TemporalPayment TP WITH (NOLOCK)
					WHERE TP.NotebookID = DVR.EmployerNotebookID
					AND DVR.ViewDate BETWEEN TP.StartDate AND TP.EndDate)
	AND EXISTS (SELECT COUNT(*) 
				FROM SRV16.RabotaUA2.dbo.DailyViewedResume DVR2 WITH (NOLOCK)
				WHERE DVR.EmployerNotebookID = DVR2.EmployerNotebookID 
				AND DVR2.ViewDate BETWEEN DATEADD(DAY, -6, DVR.ViewDate) AND DVR.ViewDate 
				AND DVR2.IsTemporalAccess = 0
				HAVING COUNT(*) &gt;= 35)
ORDER BY DVR.EmployerNotebookID;</CommandText>
      </Query>
