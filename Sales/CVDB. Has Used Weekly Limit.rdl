        <Field Name="EmployerNotebookID">
          <DataField>EmployerNotebookID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NotebookName">
          <DataField>NotebookName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Manager">
          <DataField>Manager</DataField>
