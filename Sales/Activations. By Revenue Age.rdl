  <DataSets>
    <DataSet Name="DStActivationsByRevenueAge">
      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <CommandText>SELECT DD.YearNum, DD.QuarterNum, DD.FullDate, DD.Date_key, O.NotebookID, OD.ClientPrice 
 , CASE
    WHEN EXISTS (SELECT * FROM Reporting.dbo.FactRecognizedRevenueNotebook RRN WHERE RRN.NotebookID = O.NotebookID AND RRN.Date_key BETWEEN DD.Date_key - 365 AND DD.Date_key - 1 AND RRN.RecognizedRevenue &gt; 0) THEN 'Не новые'
	ELSE 'Новые'
   END AS CompanyGroup
FROM dbo.OrderDetail OD
 JOIN dbo.Orders O ON OD.OrderID = O.ID
 JOIN Reporting.dbo.DimDate DD ON OD.ActivationStartDate = DD.FullDate
WHERE OD.State IN (2,3)
 AND ClientPrice &gt; 0
 AND O.Type IN (1,5)
ORDER BY NotebookID, YearNum, QuarterNum, FullDate;</CommandText>
      </Query>
      <Fields>
        <Field Name="YearNum">
          <DataField>YearNum</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="QuarterNum">
          <DataField>QuarterNum</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="FullDate">
          <DataField>FullDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Date_key">
          <DataField>Date_key</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NotebookID">
          <DataField>NotebookID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ClientPrice">
          <DataField>ClientPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CompanyGroup">
          <DataField>CompanyGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
