  <DataSets>
    <DataSet Name="DStPayments">
      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <CommandText>DECLARE @StartDate DATETIME = '2013-01-01';

WITH C AS
	(
SELECT 
	CASE M.DepartmentID
	WHEN 3 THEN 'Telesales'
	WHEN 4 THEN 'Telesales'
	ELSE 'Sales Force'
	END AS Department
	, DD.YearNum
	, 'Q' + CAST(DD.QuarterNum AS VARCHAR) AS QuarterNum
	, M.Name AS Manager
	, COUNT(DISTINCT NotebookID) AS NotebookCount
FROM Analytics.dbo.OrderDetail OD
	JOIN Analytics.dbo.Orders O ON OD.OrderID = O.ID
	JOIN Analytics.dbo.aspnet_Membership AM ON O.LoginEMail_OrderOwner = AM.Email
	JOIN Analytics.dbo.Manager M ON AM.UserId = M.aspnet_UserUIN
	JOIN Reporting.dbo.DimDate DD ON Analytics.dbo.fnGetDatePart(OD.ActivationStartDate) = DD.FullDate
WHERE OD.ActivationStartDate &gt;= @StartDate
	AND OD.ClientPrice &gt; 0
GROUP BY
	CASE M.DepartmentID
	WHEN 3 THEN 'Telesales'
	WHEN 4 THEN 'Telesales'
	ELSE 'Sales Force'
	END
	, DD.YearNum
	, 'Q' + CAST(DD.QuarterNum AS VARCHAR)
	, M.Name
	)

SELECT 
	Department
	, Manager
	, QuarterNum
	, ISNULL([2013],0) AS Year2013
	, ISNULL([2014],0) AS Year2014
	, ISNULL([2015],0) AS Year2015
FROM C
PIVOT
(
SUM(NotebookCount)
FOR YearNum IN ([2013],[2014],[2015])
) AS pvt;</CommandText>
      </Query>
      <Fields>
