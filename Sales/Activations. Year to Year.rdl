          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Year2013">
          <DataField>Year2013</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="QuarterNum">
          <DataField>QuarterNum</DataField>
