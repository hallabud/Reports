  <DataSets>
    <DataSet Name="DStActions">
      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ManagerIDs">
            <Value>=Parameters!ManagerIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DepartmentIDs">
            <Value>=Parameters!DepartmentIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ActionType">
            <Value>=Parameters!ActionType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT D.Name AS ActionType, A.CompleteDate, COALESCE(NC.NotebookID, MN.ID) AS NotebookID, COALESCE(NC.Name, MN.CompanyName) AS CompanyName, M.Name AS ManagerName, MU.Name AS MultiUserName, CD.ID AS DealID, D2.Name AS DealState, O.ClientPriceSum, A.ActionComment, A.ResultComment
 , (SELECT COUNT(*) FROM Analytics.dbo.NotebookCompany NC WHERE NC.ManagerID IN (@ManagerIDs)) AS ManagerCompanyNum
 , CASE WHEN NC.NotebookID IS NULL THEN 1 ELSE 0 END AS IsMetaNotebook
FROM SRV16.RabotaUA2.dbo.CRM_Action A WITH (NOLOCK)
 JOIN SRV16.RabotaUA2.dbo.CRM_Directory D WITH (NOLOCK) ON D.Type = 'ActionType' AND A.TypeID = D.ID
 LEFT JOIN SRV16.RabotaUA2.dbo.NotebookCompany NC WITH (NOLOCK) ON A.NotebookID = NC.NotebookId
 LEFT JOIN SRV16.RabotaUA2.dbo.Admin2_MetaNotebook MN WITH (NOLOCK) ON A.MetaNotebookID = MN.ID
 JOIN SRV16.RabotaUA2.dbo.aspnet_Membership AM WITH (NOLOCK) ON A.Responsible = AM.Email
 JOIN SRV16.RabotaUA2.dbo.Manager M WITH (NOLOCK) ON AM.UserId = M.aspnet_UserUIN
 JOIN SRV16.RabotaUA2.dbo.Departments DPT WITH (NOLOCK) ON M.DepartmentID = DPT.ID
 LEFT JOIN SRV16.RabotaUA2.dbo.MultiUser MU WITH (NOLOCK) ON A.MultiUserID = MU.ID
 LEFT JOIN SRV16.RabotaUA2.dbo.CRM_Deal CD WITH (NOLOCK) ON A.DealID = CD.ID
 LEFT JOIN SRV16.RabotaUA2.dbo.CRM_Directory D2 WITH (NOLOCK) ON D2.Type = 'DealState' AND CD.StateID = D2.ID
 LEFT JOIN SRV16.RabotaUA2.dbo.Orders O WITH (NOLOCK) ON A.DealID = O.DealID AND O.IsDealMain = 1
WHERE A.StateID = 2
 AND M.IsForTesting = 0
 AND DPT.ID IN (@DepartmentIDs)
 AND M.ID IN (@ManagerIDs)
 AND A.CompleteDate BETWEEN @StartDate AND @EndDate + 1
 AND A.TypeID IN (@ActionType)
ORDER BY A.CompleteDate;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
