    <DataSet Name="DStManagers">
      <Fields>
        <Field Name="ManagerName">
          <DataField>ManagerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SELECT 
  M.[Name] AS ManagerName
FROM SRV12.RabotaUA2.dbo.Manager M
WHERE M.Name NOT IN ('QA','Дмитрий','telesales','Вадим','Иван')
 AND EXISTS (SELECT * FROM SRV12.RabotaUA2.dbo.NotebookCompany NC WHERE ManagerId = M.Id 
				AND NOT EXISTS (SELECT * FROM SRV12.RabotaUA2.dbo.NotebookCompanyMerged NCM WHERE NC.NotebookId = NCM.SourceNotebookId))
ORDER BY Surname</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
