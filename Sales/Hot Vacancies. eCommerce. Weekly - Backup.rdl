          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="SumPackageCount">
          <DataField>SumPackageCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SumClientPrice">
          <DataField>SumClientPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>WITH C AS 
 (
SELECT 
   CASE OS.PackageCount WHEN 1 THEN 'Горячая - 1' WHEN 4 THEN 'Горячая - 4' WHEN 8 THEN 'Горячая - 8' END AS PackageType
 , OD.Count AS PackageCount
 , OD.ClientPrice
 , OD.OrderID
 , O.NotebookID
 , OD.ActivationStartDate
 , DD.YearNum
 , DD.MonthNum
 , DD.MonthNameRus
 , DD.WeekNum
 , DD.WeekName
 , CASE 
    WHEN U.UserName = 'JuliyaO@rabota.ua' AND OD.ActivationStartDate &lt; '2014-01-13' THEN 'Enterprise Channel'
	WHEN D.Id = 3 THEN 'SMB Channel'
	ELSE 'Enterprise Channel'
   END AS 'Department'
 , M.Name
FROM Analytics.dbo.Orders O
 JOIN Analytics.dbo.OrderDetail OD ON O.ID = OD.OrderID
 JOIN Analytics.dbo.OrderService OS ON OD.ServiceID = OS.ID
 JOIN Analytics.dbo.OrderService_Unit OSU ON OD.UnitID = OSU.ID
 JOIN Analytics.dbo.aspnet_Users U ON O.LoginEMail_OrderOwner = U.UserName
 JOIN Analytics.dbo.Manager M ON U.UserId = M.aspnet_UserUIN
 JOIN Analytics.dbo.Departments D ON M.DepartmentID = D.Id
 JOIN Reporting.dbo.DimDate DD ON OD.ActivationStartDate = DD.FullDate
WHERE O.State IN (4,5)
 AND OD.ServiceID IN (23,75,76,92,93,94)
 AND OD.ClientPrice &gt; 0
 AND D.ID &lt;&gt; 5
 AND O.Type = 5
 AND EXISTS (SELECT * 
			 FROM Analytics.dbo.Order_Acc OA
			  JOIN Analytics.dbo.Order_AccPayment OAP ON OA.Year = OAP.AccYear AND OA.ID = OAP.AccID
			 WHERE OA.OrderID = O.ID)
 )
SELECT 
   PackageType
 , Department
 , YearNum
 , MonthNum
 , MonthNameRus
 , WeekNum
 , WeekName
 , ActivationStartDate
 , SUM(PackageCount) AS SumPackageCount
 , SUM(ClientPrice) AS SumClientPrice
FROM C
WHERE ActivationStartDate BETWEEN @StartDate AND @EndDate
GROUP BY 
   PackageType
 , Department
 , YearNum
 , MonthNum
 , MonthNameRus
 , WeekNum
 , WeekName
 , ActivationStartDate</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!Prm_StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!Prm_EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
