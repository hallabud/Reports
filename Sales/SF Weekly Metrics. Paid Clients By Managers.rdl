  <DataSets>
    <DataSet Name="DStPaidClients">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@YearNum">
            <Value>=Parameters!YearNum.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@QuarterNum">
            <Value>=Parameters!QuarterNum.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Department">
            <Value>=Parameters!Department.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH C AS
 (
SELECT DD.YearNum, DD.QuarterNum, DD.WeekNum, DD.WeekName, D.Name AS DepartmentName, M.ID AS ManagerID, M.Name AS ManagerName, 'Команда "' +  RTRIM(COALESCE(TL.Name, M.Name)) + '"' AS TeamName, COUNT(DISTINCT FCS.Company_key) AS PaidCompanyCount
FROM dbo.DimDate DD
 JOIN dbo.FactCompanyStatuses FCS ON DD.Date_key = FCS.Date_key
 JOIN dbo.DimCompany DC ON FCS.Company_key = DC.Company_key
 JOIN Analytics.dbo.Manager M ON DC.ManagerName = M.Name
 JOIN Analytics.dbo.Departments D ON M.DepartmentID = D.ID
 JOIN Analytics.dbo.Manager TL ON M.TeamLead_ManagerID = TL.ID
WHERE YearNum = @YearNum AND QuarterNum = @QuarterNum
 AND FCS.HasPaidServices = 1
 AND M.DepartmentID IN (@Department)
 AND M.IsForTesting = 0
 AND M.IsReportExcluding = 0
GROUP BY DD.YearNum, DD.QuarterNum, DD.WeekNum, DD.WeekName, D.Name, M.ID, M.Name, TL.Name
 )
SELECT C.*, ISNULL(TQ.Clients,0) AS TargetClients
FROM C
 LEFT JOIN dbo.TargetsQuarter TQ ON C.YearNum = TQ.YearNum AND C.WeekNum = TQ.WeekNum AND C.ManagerID = TQ.ManagerID AND C.QuarterNum = TQ.QuarterNum</CommandText>
      </Query>
      <Fields>
        <Field Name="YearNum">
          <DataField>YearNum</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="QuarterNum">
          <DataField>QuarterNum</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="WeekNum">
