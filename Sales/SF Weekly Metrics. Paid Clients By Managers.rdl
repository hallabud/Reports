          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TeamName">
          <DataField>TeamName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ManagerID">
          <DataField>ManagerID</DataField>
