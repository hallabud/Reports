  <DataSets>
    <DataSet Name="DStBonuses">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

IF OBJECT_ID('tempdb..#C') IS NOT NULL DROP TABLE #C;
IF OBJECT_ID('tempdb..#NotebookCompany_history','U') IS NOT NULL DROP TABLE #NotebookCompany_history;
IF OBJECT_ID('tempdb..#ResultTable','U') IS NOT NULL DROP TABLE #ResultTable;

SELECT D.ID AS DealID, D.CloseDate, O.ID AS OrderID,  O.NotebookID, O.ClientPriceSum, O.LoginEMail_OrderOwner, MIN(OAP.PayDate) AS FirstPayDate
INTO #C
FROM SRV16.RabotaUA2.dbo.CRM_Deal D
 JOIN SRV16.RabotaUA2.dbo.Orders O ON D.ID = O.DealID
 JOIN SRV16.RabotaUA2.dbo.Order_Acc OA ON O.ID = OA.OrderID
 JOIN SRV16.RabotaUA2.dbo.Order_AccPayment OAP ON OA.ID = OAP.AccID AND OA.Year = OAP.AccYear
WHERE D.StateID = 2 AND O.IsDealMain = 1
GROUP BY D.ID, D.CloseDate, O.ID, O.NotebookID, O.ClientPriceSum, O.LoginEMail_OrderOwner;

SELECT * 
INTO #NotebookCompany_history
FROM SRV16.RabotaUA2.dbo.NotebookCompany_history NCH 
WHERE NCH.Field = 'ManagerID' AND DateOfUpd &gt;= @StartDate;

SELECT MO.Name AS ManagerOld, MN.Name AS ManagerNew, NCH.DateOfUpd AS TransferDate, NCH.NotebookId, NC.Name AS CompanyName
 , C.CloseDate
 , C.FirstPayDate
 , C.OrderID
 , C.ClientPriceSum
 , CONVERT(DECIMAL(16,2), 0.07 * C.ClientPriceSum) AS ManagerBonusSum
 , ROW_NUMBER() OVER(PARTITION BY C.NotebookID ORDER BY C.CloseDate) AS RowNum
INTO #ResultTable
FROM #NotebookCompany_history NCH
 JOIN SRV16.RabotaUA2.dbo.Manager MO ON NCH.OldValue = MO.Id
 JOIN SRV16.RabotaUA2.dbo.aspnet_Membership MMB ON MO.aspnet_UserUIN = MMB.UserId
 JOIN SRV16.RabotaUA2.dbo.Manager MN ON NCH.NewValue = MN.Id
 JOIN SRV16.RabotaUA2.dbo.NotebookCompany NC ON NCH.NotebookId = NC.NotebookId
 JOIN #C C ON NCH.NotebookId = C.NotebookID
WHERE MO.DepartmentID = 2 AND MO.IsLoyaltyGroup = 1
 AND MN.DepartmentID = 2 AND MN.IsLoyaltyGroup &lt;&gt; 1
 AND EXISTS (SELECT * FROM SRV16.RabotaUA2.dbo.CRM_Action A WHERE A.StateID = 2 AND A.Responsible = MMB.Email AND A.NotebookID = NCH.NotebookId AND A.CompleteDate BETWEEN DATEADD(DAY, -30, dbo.fnGetDatePart(NCH.DateOfUpd)) AND NCH.DateOfUpd)
 AND FirstPayDate BETWEEN dbo.fnGetDatePart(NCH.DateOfUpd) AND DATEADD(MONTH, 3, dbo.fnGetDatePart(NCH.DateOfUpd));

SELECT * 
FROM #ResultTable 
WHERE RowNum = 1;</CommandText>
