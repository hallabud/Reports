    <DataSet Name="DStPotential">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>DECLARE @TodayDate DATETIME; SET @TodayDate = dbo.fnGetDatePart(GETDATE());

WITH CTE AS 
 (
SELECT  
   YearNum
 , MonthNum
 , MonthNameRus
 , FullDate
 , DayNum
 , IsMegaChecked
 , HasPaidServices
 , MonthsFromRegDate
 , IsActive2
 , Company_key
FROM dbo.vw_CompaniesActivityGroups_Active2 vw
WHERE IsMegachecked = 1 OR HasPaidServices = 1
 )
, CTE_Companies AS 
 (
SELECT
   YearNum
 , MonthNum
 , MonthNameRus
 , FullDate
 , DayNum
 , MonthsFromRegDate
 , IsActive2
 , Company_key
 , ROW_NUMBER() OVER(PARTITION BY Company_key ORDER BY FullDate) AS DateOrdNum
FROM CTE
WHERE DayNum = 1 OR FullDate = @TodayDate
 )
, CTE_PotentialActive2 AS
 (
SELECT
   C1.YearNum
 , C1.MonthNum
 , C1.MonthNameRus
 , C1.FullDate
 , C1.DayNum
 , C1.MonthsFromRegDate
 , C1.IsActive2
 , C1.Company_key
 , C1.DateOrdNum 
 , C2.FullDate AS FullDate_Next
 , C2.DateOrdNum AS DateOrdNum_Next
 , C2.Company_key AS Company_key_Next
FROM CTE_Companies C1
 LEFT JOIN CTE_Companies C2 ON C1.Company_key = C2.Company_key AND C1.DateOrdNum = C2.DateOrdNum - 1 AND C2.IsActive2 = 1
WHERE C1.IsActive2 = 1
 )
, CTE_Potential_New AS
 (
SELECT
   C1.YearNum
 , C1.MonthNum
 , C1.MonthNameRus
 , C1.FullDate
 , C1.DayNum
 , C1.MonthsFromRegDate
 , C1.IsActive2
 , C1.Company_key
 , C1.DateOrdNum 
 , C2.FullDate AS FullDate_Next
 , C2.DateOrdNum AS DateOrdNum_Next
 , C2.Company_key AS Company_key_Next
FROM CTE_Companies C1
 LEFT JOIN CTE_Companies C2 ON C1.Company_key = C2.Company_key AND C1.DateOrdNum = C2.DateOrdNum - 1 AND C2.IsActive2 = 1
WHERE C1.IsActive2 = 0 
 AND C1.MonthsFromRegDate &lt; 2
 AND EXISTS (SELECT * 
			 FROM dbo.FactCompanyStatuses CS 
			 WHERE C1.Company_key = CS.Company_key
			  AND PublicationsNumLast2Months &gt; 1)
 )
, CTE_Potential_Comeback AS
 (
SELECT
   C1.YearNum
 , C1.MonthNum
 , C1.MonthNameRus
 , C1.FullDate
 , C1.DayNum
 , C1.MonthsFromRegDate
 , C1.IsActive2
 , C1.Company_key
 , C1.DateOrdNum 
 , C2.FullDate AS FullDate_Next
 , C2.DateOrdNum AS DateOrdNum_Next
 , C2.Company_key AS Company_key_Next
FROM CTE_Companies C1
 LEFT JOIN CTE_Companies C2 ON C1.Company_key = C2.Company_key AND C1.DateOrdNum = C2.DateOrdNum - 1 AND C2.IsActive2 = 1
WHERE C1.IsActive2 = 0 
 AND C1.MonthsFromRegDate &gt;= 2
 AND EXISTS (SELECT * 
                       FROM dbo.vw_CompaniesActivityGroups_Active2 vw 
                       WHERE vw.Company_key = C1.Company_key
                        AND vw.IsActive2 = 1
                        AND vw.FullDate &lt; C1.FullDate)
 )
--SELECT TOP 1000 * FROM CTE_IsActive2

SELECT 
   'Потенциал: Удержанные' AS PotentialGroup
 , FullDate
 , MonthNameRus
 , COUNT(DISTINCT Company_key) AS CurMonth
 , COUNT(DISTINCT Company_key_Next) AS NextMonth
 , 1. * COUNT(DISTINCT Company_key_Next) / COUNT(DISTINCT Company_key) AS Conversion
FROM CTE_PotentialActive2
WHERE FullDate &gt;= '2013-04-01' AND FullDate &lt; @TodayDate
GROUP BY FullDate, MonthNameRus

UNION ALL

SELECT 
   'Потенциал: Новые'
 , FullDate
 , MonthNameRus
 , COUNT(DISTINCT Company_key) AS CurMonth
 , COUNT(DISTINCT Company_key_Next) AS NextMonth
 , 1. * COUNT(DISTINCT Company_key_Next) / COUNT(DISTINCT Company_key) AS Conversion
FROM CTE_Potential_New
WHERE FullDate &gt;= '2013-04-01' AND FullDate &lt; @TodayDate
GROUP BY FullDate, MonthNameRus

UNION ALL

SELECT 
   'Потенциал: Возвращенные'
 , FullDate
 , MonthNameRus
 , COUNT(DISTINCT Company_key) AS CurMonth
 , COUNT(DISTINCT Company_key_Next) AS NextMonth
 , 1. * COUNT(DISTINCT Company_key_Next) / COUNT(DISTINCT Company_key) AS Conversion
FROM CTE_Potential_Comeback
WHERE FullDate &gt;= '2013-04-01' AND FullDate &lt; @TodayDate
GROUP BY FullDate, MonthNameRus;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
