    <DataSet Name="DStReporting">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>DECLARE @TodayDate DATETIME; SET @TodayDate = dbo.fnGetDatePart(GETDATE());

WITH CTE AS 
 (
SELECT  
   YearNum
 , MonthNum
 , MonthNameRus
 , FullDate + 1 AS FullDate
 , DayNum
 , WeekDayNum
 , IsMegaChecked
 , HasPaidServices
 , CASE 
    WHEN MonthsFromRegDate &lt;= 2 THEN 'Новая'
    WHEN EXISTS (SELECT * 
	             FROM dbo.vw_CompaniesActivityGroups_Active2 vw2
	             WHERE vw2.Company_key = vw.Company_key
                              AND vw2.FullDate = DATEADD(MONTH, -1, vw.FullDate)
	              AND vw2.IsActive2 = 1) THEN 'Удержанная'
   ELSE 'Возвращенная'
   END AS 'CompanyGrp'
 , Company_key
FROM dbo.vw_CompaniesActivityGroups_Active2 vw
WHERE vw.IsActive2 = 1 AND vw.FullDate &gt;= '2013-04-01'
 )
SELECT 
   YearNum
 , MonthNum
 , MonthNameRus
 , FullDate
 , IsMegaChecked
 , HasPaidServices
 , CompanyGrp
 , COUNT(DISTINCT Company_key) AS CompaniesNum
FROM CTE
WHERE (IsMegachecked = 1 OR HasPaidServices = 1)
 AND (WeekDayNum = 5 OR FullDate = @TodayDate)
GROUP BY
   YearNum
 , MonthNum
 , MonthNameRus
 , FullDate
 , IsMegaChecked
 , HasPaidServices
 , CompanyGrp
 ;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
