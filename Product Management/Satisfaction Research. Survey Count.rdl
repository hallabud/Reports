  <DataSets>
    <DataSet Name="DStMain">
      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF OBJECT_ID('tempdb..#Surveys','U') IS NOT NULL DROP TABLE #Surveys;
IF OBJECT_ID('tempdb..#AvgDaily','U') IS NOT NULL DROP TABLE #AvgDaily;
IF OBJECT_ID('tempdb..#AvgWeekly','U') IS NOT NULL DROP TABLE #AvgWeekly;
IF OBJECT_ID('tempdb..#AvgMonthly','U') IS NOT NULL DROP TABLE #AvgMonthly;

SELECT D.Name AS Department, M.Name AS Manager
 , CASE WHEN VA.IsByManager = 1 THEN 'Телефон' ELSE 'Email' END AS AnketaSource
 , DD.YearNum, DD.MonthNum, DD.MonthNameRus, DD.WeekNum, DD.WeekName, DD.FullDate, DD.WeekDayNum, DD.WeekDayNameRus
 , COUNT(*) AS SurveyCount
INTO #Surveys
FROM dbo.CRM_VacancyAnketa VA
 JOIN Reporting.dbo.DimDate DD ON dbo.fnGetDatePart(VA.AddDate) = DD.FullDate
 LEFT JOIN dbo.aspnet_Membership MMB ON VA.LoginEMail = MMB.Email
 LEFT JOIN dbo.Manager M ON MMB.UserId = M.aspnet_UserUIN
 LEFT JOIN dbo.Departments D ON M.DepartmentID = D.ID
WHERE VA.AddDate BETWEEN @StartDate AND @EndDate + 1
GROUP BY 
   D.Name, M.Name
 , CASE WHEN VA.IsByManager = 1 THEN 'Телефон' ELSE 'Email' END
 , DD.YearNum, DD.MonthNum, DD.MonthNameRus, DD.WeekNum, DD.WeekName, DD.FullDate, DD.WeekDayNum, DD.WeekDayNameRus
 
SELECT Department, FullDate, AVG(SurveyCount) AS AvgSurveyCountDaily
INTO #AvgDaily
FROM #Surveys
WHERE Department IN ('Telesales','Sales Force')
GROUP BY Department, FullDate
 
SELECT DISTINCT Department, YearNum, WeekNum, AVG(SUM(SurveyCount))  OVER(PARTITION BY Department, YearNum, WeekNum) AS AvgSurveyCountWeekly
INTO #AvgWeekly
FROM #Surveys
WHERE Department IN ('Telesales','Sales Force')
GROUP BY Department, Manager, YearNum, WeekNum

SELECT DISTINCT Department, YearNum, MonthNum, AVG(SUM(SurveyCount)) OVER(PARTITION BY Department, YearNum, MonthNum) AS AvgSurveyCountMonthly
INTO #AvgMonthly
FROM #Surveys
WHERE Department IN ('Telesales','Sales Force')
GROUP BY Department, Manager, YearNum, MonthNum

SELECT * 
 , (SELECT AvgSurveyCountDaily FROM #AvgDaily WHERE FullDate = S.FullDate AND Department = S.Department) AS AvgSurveyCountDaily
 , (SELECT AvgSurveyCountWeekly FROM #AvgWeekly WHERE YearNum = S.YearNum AND WeekNum = S.WeekNum AND Department = S.Department) AS AvgSurveyCountWeekly
 , (SELECT AvgSurveyCountMonthly FROM #AvgMonthly WHERE YearNum = S.YearNum AND MonthNum = S.MonthNum AND Department = S.Department) AS AvgSurveyCountMonthly
FROM #Surveys S;

DROP TABLE #Surveys;
DROP TABLE #AvgDaily;
DROP TABLE #AvgWeekly;
DROP TABLE #AvgMonthly;</CommandText>
