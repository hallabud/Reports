  <DataSets>
    <DataSet Name="DStAnketaVacancyList">
      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PollChannel">
            <Value>=Parameters!PollChannel.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ParentGroupID">
            <Value>=Parameters!ParentGroupID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@City">
            <Value>=Parameters!City.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProfLevel">
            <Value>=Parameters!ProfLevel.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StarRating">
            <Value>=Parameters!StarRating.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@VacancyType">
            <Value>=Parameters!VacancyType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WorkEvaluated">
            <Value>=Parameters!WorkEvaluated.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RabotaEvaluated">
            <Value>=Parameters!RabotaEvaluated.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH TagIDs AS
 (
SELECT S.ID AS TagID
FROM Reporting.dbo.Lookup_ParentGroup_SynonymGroup PGSG
 JOIN Reporting.dbo.Lookup_ParentGroups PG ON PGSG.ParentGroupID = PG.ParentGroupID
 JOIN Analytics.dbo.Synonymous S ON S.SynonymousID = PGSG.SynonymousID
WHERE PGSG.ParentGroupID = @ParentGroupID
 )
, C AS
 (
SELECT DISTINCT 
   DD.FullDate
 , DD.YearNum
 , DD.WeekNum
 , DD.WeekName
 , VA.NotebookID
 , NC.Name AS CompanyName
 , NCE.RatingByMisha AS StarRating
 , VA.ID AS VacancyAnketaID 
 , VA.VacancyID
 , VA.VacancyName
 , VA.IsByManager
 , VA.ResponseCount
 , VA.KeyPZIDs
 , VA.SearchPZIDs
 , PL.Name AS ProfLevel
 , C.Name AS City
 , COALESCE(OblC.Name, C.Name) AS OblCenter
 , M.Name AS Manager
 , A.QuestionID
 , CASE A.QuestionID
    WHEN 1 THEN 'Насколько Вы удовлетворены текущим результатом размещения вакансии на rabota.ua?'
	WHEN 2 THEN 'Оцените по тем же критериям другие ресурсы, на которых Вы размещали вакансию?'
	WHEN 3 THEN 'Вы закрыли вакансию?'
   END AS QuestionName
 , CASE 
    WHEN A.QuestionID = 1 THEN 'rabota.ua'
	ELSE A.Name
   END AS Answer1Level
 , ISNULL((CASE 
    WHEN A.QuestionID = 1 THEN A.Name
	ELSE (SELECT TOP 1 A2.Name 
		  FROM CRM_VacancyAnketa VA2
		   JOIN dbo.CRM_VacancyAnketa_Answer VAA2 ON VA2.ID = VAA2.VacancyAnketaID
	       JOIN dbo.CRM_Anketa A2 ON VAA2.AnketaID = A2.ID
	      WHERE VA2.ID = VA.ID AND A2.ParentID = A.ID) 
   END),'Без оценки') AS Answer2Level
FROM dbo.CRM_VacancyAnketa VA
 JOIN dbo.NotebookCompany NC ON VA.NotebookID = NC.NotebookId
 JOIN dbo.NotebookCompanyExtra NCE ON NC.NotebookId = NCE.NotebookID
 JOIN dbo.VacancyCity VC ON VA.VacancyID = VC.VacancyId
 JOIN dbo.City C ON VC.CityId = C.Id
 LEFT JOIN dbo.City OblC ON C.OblastCityID = OblC.Id
 JOIN dbo.Manager M ON NC.ManagerId = M.Id AND M.IsForTesting = 0
 JOIN dbo.CRM_VacancyAnketa_Answer VAA ON VA.ID = VAA.VacancyAnketaID
 JOIN dbo.CRM_Anketa A ON VAA.AnketaID = A.ID
 LEFT JOIN dbo.ProfLevel PL ON VA.ProfLevelID = PL.ID
 JOIN Reporting.dbo.DimDate DD ON dbo.fnGetDatePart(VA.AddDate) = DD.FullDate
WHERE A.ParentID IS NULL AND QuestionID IN (1,2)
 AND CAST(VA.IsByManager AS tinyint) IN (@PollChannel)
 AND C.Name IN (@City)
 AND PL.Name IN (@ProfLevel)
 AND NCE.RatingByMisha IN (@StarRating) 
 AND (EXISTS (SELECT * FROM TagIDs T WHERE CHARINDEX(',' + CAST(T.TagID AS varchar) + ',', ',' + VA.KeyPZIDs +',') &gt; 0) OR 1 = CASE WHEN @ParentGroupID = 0 THEN 1 ELSE 0 END)
 AND DD.FullDate BETWEEN @StartDate AND @EndDate
 AND VA.VacancyType IN (@VacancyType)
 )
SELECT 
   FullDate, YearNum, WeekNum, WeekName, NotebookID, CompanyName, StarRating, VacancyAnketaID, VacancyID, VacancyName, IsByManager, ResponseCount, KeyPZIDs, SearchPZIDs, ProfLevel, City, OblCenter, Manager /*, QuestionID, QuestionName*/
 , [rabota.ua] AS 'rabota.ua'
 , [work.ua] AS 'work.ua'
 , [hh.ua] AS 'hh.ua'
 , [slando.ua] AS 'slando.ua'
FROM 
(
SELECT FullDate, YearNum, WeekNum, WeekName, NotebookID, CompanyName, StarRating, VacancyAnketaID, VacancyID, VacancyName, IsByManager, ResponseCount, KeyPZIDs, SearchPZIDs, ProfLevel, City, OblCenter, Manager, Answer1Level, Answer2Level
FROM C
WHERE EXISTS (SELECT * FROM C C2 WHERE C2.VacancyAnketaID = C.VacancyAnketaID AND C2.Answer1Level = CASE WHEN @WorkEvaluated = 1 THEN 'work.ua' ELSE C2.Answer1Level END)
 AND EXISTS (SELECT * FROM C C3 WHERE C3.VacancyAnketaID = C.VacancyAnketaID AND C3.Answer1Level = CASE WHEN @RabotaEvaluated = 1 THEN 'rabota.ua' ELSE C3.Answer1Level END)
) AS SourceTable
PIVOT
(
MIN(Answer2Level)
FOR Answer1Level IN ([rabota.ua],[work.ua],[hh.ua],[slando.ua])
) AS PivotTable</CommandText>
