    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DStTest">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>WITH C AS
 (
SELECT COALESCE(DR.OblastCityID, DR.AnalyticsDbId) AS CityID
 , (SELECT COUNT(*) 
    FROM Analytics.dbo.VacancyCity VC
	 JOIN Analytics.dbo.VacancyPublished VP ON VC.VacancyId = VP.ID
	WHERE VC.CityId = DR.AnalyticsDbId) AS VacancyCount
FROM dbo.DimRegion DR 
 )
SELECT Region_key, AnalyticsDbId, NameRus, GeoLocation
 , (SELECT SUM(VacancyCount) FROM C WHERE C.CityID = DR.AnalyticsDbId) AS VacancyCount
FROM dbo.DimRegion DR 
WHERE OblastCityID IS NULL 
 AND GeoLocation IS NOT NULL
 ORDER BY VacancyCount DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="NameRus">
          <DataField>NameRus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Region_key">
          <DataField>Region_key</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AnalyticsDbId">
          <DataField>AnalyticsDbId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="GeoLocation">
          <DataField>GeoLocation</DataField>
          <rd:TypeName>Microsoft.SqlServer.Types.SqlGeography</rd:TypeName>
        </Field>
        <Field Name="VacancyCount">
          <DataField>VacancyCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
