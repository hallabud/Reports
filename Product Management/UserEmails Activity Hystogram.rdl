  <DataSets>
    <DataSet Name="DStUserEmailActivity">
      <Query>
        <DataSourceName>DSrcRabotaDWH</DataSourceName>
        <CommandText>WITH C AS 
 (
SELECT 
   D.FullDate
 , CASE 
	WHEN UEA.VacancyApplyNum &gt; 0 AND ActiveProfessionalCvCreated = 0 AND ActiveProfessionalCvUpdated = 0 THEN 'Сделали хоть один отклик и нет созданного или обновленного резюме'
	WHEN UEA.VacancyApplyNum &gt; 0 AND (ActiveProfessionalCvCreated = 1 OR ActiveProfessionalCvUpdated = 1) THEN 'Сделали хоть один отклик и есть созданное или обновленное резюме'
	WHEN UEA.VacancyApplyNum = 0 AND (ActiveProfessionalCvCreated = 1 OR ActiveProfessionalCvUpdated = 1) THEN 'Есть созданное или обновленное резюме, но нет откликов'
	WHEN UEA.VacancyApplyNum = 0 AND ActiveProfessionalCvCreated = 0 AND ActiveProfessionalCvUpdated = 0 AND (JobAlertLetterRead = 1 OR JobAlertLetterClick = 1) THEN 'Не откликались, нет созданного или обновленного резюме, но есть просмотры или переходы по jobalert'
   END AS UserEmailGroup
 , CASE 
    WHEN UEA.VacancyApplyNum &gt; 0 AND ActiveProfessionalCvCreated = 0 AND ActiveProfessionalCvUpdated = 0 THEN 1
	WHEN UEA.VacancyApplyNum &gt; 0 AND (ActiveProfessionalCvCreated = 1 OR ActiveProfessionalCvUpdated = 1) THEN 2
	WHEN UEA.VacancyApplyNum = 0 AND (ActiveProfessionalCvCreated = 1 OR ActiveProfessionalCvUpdated = 1) THEN 3
	WHEN UEA.VacancyApplyNum = 0 AND ActiveProfessionalCvCreated = 0 AND ActiveProfessionalCvUpdated = 0 AND (JobAlertLetterRead = 1 OR JobAlertLetterClick = 1) THEN 4
   END AS UserEmailGroupOrderNum
 , COUNT(DISTINCT EmailKey) AS UserEmailCount
FROM dbo.FactUserEmailActivity UEA
 JOIN dbo.DimDate D ON UEA.DateKey = D.DateKey
GROUP BY
   D.FullDate
 , CASE 
	WHEN UEA.VacancyApplyNum &gt; 0 AND ActiveProfessionalCvCreated = 0 AND ActiveProfessionalCvUpdated = 0 THEN 'Сделали хоть один отклик и нет созданного или обновленного резюме'
	WHEN UEA.VacancyApplyNum &gt; 0 AND (ActiveProfessionalCvCreated = 1 OR ActiveProfessionalCvUpdated = 1) THEN 'Сделали хоть один отклик и есть созданное или обновленное резюме'
	WHEN UEA.VacancyApplyNum = 0 AND (ActiveProfessionalCvCreated = 1 OR ActiveProfessionalCvUpdated = 1) THEN 'Есть созданное или обновленное резюме, но нет откликов'
	WHEN UEA.VacancyApplyNum = 0 AND ActiveProfessionalCvCreated = 0 AND ActiveProfessionalCvUpdated = 0 AND (JobAlertLetterRead = 1 OR JobAlertLetterClick = 1) THEN 'Не откликались, нет созданного или обновленного резюме, но есть просмотры или переходы по jobalert'
   END
 , CASE 
    WHEN UEA.VacancyApplyNum &gt; 0 AND ActiveProfessionalCvCreated = 0 AND ActiveProfessionalCvUpdated = 0 THEN 1
	WHEN UEA.VacancyApplyNum &gt; 0 AND (ActiveProfessionalCvCreated = 1 OR ActiveProfessionalCvUpdated = 1) THEN 2
	WHEN UEA.VacancyApplyNum = 0 AND (ActiveProfessionalCvCreated = 1 OR ActiveProfessionalCvUpdated = 1) THEN 3
	WHEN UEA.VacancyApplyNum = 0 AND ActiveProfessionalCvCreated = 0 AND ActiveProfessionalCvUpdated = 0 AND (JobAlertLetterRead = 1 OR JobAlertLetterClick = 1) THEN 4
   END
 )
SELECT * 
FROM C
WHERE UserEmailGroup IS NOT NULL
ORDER BY FullDate, UserEmailGroupOrderNum</CommandText>
      </Query>
      <Fields>
        <Field Name="FullDate">
          <DataField>FullDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="UserEmailGroup">
          <DataField>UserEmailGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UserEmailGroupOrderNum">
          <DataField>UserEmailGroupOrderNum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="UserEmailCount">
          <DataField>UserEmailCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DStOnlyJobalertProfCvExistance">
      <Query>
        <DataSourceName>DSrcRabotaDWH</DataSourceName>
        <CommandText>WITH C AS 
 (
SELECT 
   D.FullDate
 , CASE 
	WHEN ActiveProfessionalCvNum &gt; 0 THEN 'Есть активное профессиональное резюме'
	ELSE 'Нет активного профессионального резюме'
   END AS ProfCvGroup
 , CASE 
	WHEN ActiveProfessionalCvNum &gt; 0 THEN 1
	ELSE 2
   END AS ProfCvGroupOrderNum
 , COUNT(DISTINCT EmailKey) AS UserEmailCount
FROM dbo.FactUserEmailActivity UEA
 JOIN dbo.DimDate D ON UEA.DateKey = D.DateKey
WHERE UEA.VacancyApplyNum = 0 AND ActiveProfessionalCvCreated = 0 AND ActiveProfessionalCvUpdated = 0 AND (JobAlertLetterRead = 1 OR JobAlertLetterClick = 1)
GROUP BY
   D.FullDate
 , CASE 
	WHEN ActiveProfessionalCvNum &gt; 0 THEN 'Есть активное профессиональное резюме'
	ELSE 'Нет активного профессионального резюме'
   END
 , CASE 
	WHEN ActiveProfessionalCvNum &gt; 0 THEN 1
	ELSE 2
   END
 )
SELECT * 
FROM C
ORDER BY FullDate, ProfCvGroupOrderNum</CommandText>
      </Query>
      <Fields>
        <Field Name="FullDate">
          <DataField>FullDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
