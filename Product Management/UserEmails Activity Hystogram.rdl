          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProfCvGroupOrderNum">
          <DataField>ProfCvGroupOrderNum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="UserEmailCount">
          <DataField>UserEmailCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
