    <DataSet Name="DStMain">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@q">
            <Value>=Parameters!q.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Y">
            <Value>=Parameters!Y.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MngID">
            <Value>=Parameters!MngID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>	IF object_id('tempdb..#orders','U') is not null drop table #orders;
       select o.notebookid, dc.company_key, o.id orderID, oap.id [billID], oap.AddDate paydate, (oap.paysum) PaySum, dch.managerid
	   into #orders
	          from srv16.rabotaua2.dbo.Orders o with(nolock) 
              join srv16.rabotaua2.dbo.Order_Acc oa with(nolock)  ON o.id=oa.orderid
              join srv16.rabotaua2.dbo.Order_AccPayment oap with(nolock) ON oap.accid=oa.id AND oap.AccYear = oa.Year-- AND oap.adddate between '2018-01-01' and GETDATE()
              join Reporting.dbo.DimCompany dc ON o.notebookid=dc.NotebookId
			  join [dbo].[DimCompanyHistory] dch ON dc.Company_key=dch.Company_key and /*-!-*/ getdate() between dch.StartDate and dch.EndDate
		where oap.adddate&gt;='01.01.2013'
		order by managerid

/**********************************************************************************/
	
	if object_id('tempdb..#result','U') is not null drop table #result;
	select 
		year(paydate) Yearnum,
		datepart(quarter, paydate) [QNum],
		notebookid,
		managerid,
		orderid orderid,
		billID,
		PaySum
	into #result
	from #orders o
	order by 1,2,3

select * from #result
	where qnum=@q and yearnum=@Y and ManagerID=Isnull(@MngID, 1)</CommandText>
      </Query>
