 , VA.ID AS VacancyAnketaID
 , VA.VacancyID
 , VA.VacancyName
 , VA.IsByManager
 , VA.ResponseCount
 , VA.KeyPZIDs
 , VA.SearchPZIDs
 , PL.Name AS ProfLevel
 , C.Name AS City
 , COALESCE(OblC.Name, C.Name) AS OblCenter
 , M.Name AS Manager
 , A.QuestionID
 , CASE A.QuestionID
    WHEN 1 THEN 'Насколько Вы удовлетворены текущим результатом размещения вакансии на rabota.ua?'
	WHEN 2 THEN 'Оцените по тем же критериям другие ресурсы, на которых Вы размещали вакансию?'
	WHEN 3 THEN 'Вы закрыли вакансию?'
   END AS QuestionName
 , CASE 
    WHEN A.QuestionID = 1 THEN 'rabota.ua'
	ELSE A.Name
   END AS Answer1Level
 , ISNULL((CASE 
    WHEN A.QuestionID = 1 THEN A.Name
	ELSE (SELECT TOP 1 A2.Name 
		  FROM CRM_VacancyAnketa VA2
		   JOIN dbo.CRM_VacancyAnketa_Answer VAA2 ON VA2.ID = VAA2.VacancyAnketaID
	       JOIN dbo.CRM_Anketa A2 ON VAA2.AnketaID = A2.ID
	      WHERE VA2.ID = VA.ID AND A2.ParentID = A.ID) 
   END),'Без оценки') AS Answer2Level
FROM dbo.CRM_VacancyAnketa VA
 JOIN dbo.NotebookCompany NC ON VA.NotebookID = NC.NotebookId
 JOIN dbo.NotebookCompanyExtra NCE ON NC.NotebookId = NCE.NotebookID
 JOIN dbo.VacancyCity VC ON VA.VacancyID = VC.VacancyId
 JOIN dbo.City C ON VC.CityId = C.Id
 LEFT JOIN dbo.City OblC ON C.OblastCityID = OblC.Id
 JOIN dbo.Manager M ON NC.ManagerId = M.Id AND M.IsForTesting = 0
 JOIN dbo.CRM_VacancyAnketa_Answer VAA ON VA.ID = VAA.VacancyAnketaID
 JOIN dbo.CRM_Anketa A ON VAA.AnketaID = A.ID
 LEFT JOIN dbo.ProfLevel PL ON VA.ProfLevelID = PL.ID
 JOIN Reporting.dbo.DimDate DD ON dbo.fnGetDatePart(VA.AddDate) = DD.FullDate
WHERE A.ParentID IS NULL AND QuestionID IN (1,2)
 AND CAST(VA.IsByManager AS tinyint) IN (@PollChannel)
 AND C.Name IN (@City)
 AND PL.Name IN (@ProfLevel)
 AND NCE.RatingByMisha IN (@StarRating) 
 AND (EXISTS (SELECT * FROM TagIDs T WHERE CHARINDEX(',' + CAST(T.TagID AS varchar) + ',', ',' + VA.KeyPZIDs +',') &gt; 0) OR 1 = CASE WHEN @ParentGroupID = 0 THEN 1 ELSE 0 END)
 AND DD.FullDate BETWEEN @StartDate AND @EndDate
 AND VA.VacancyType IN (@VacancyType)
 )
 , C2 AS
 (
SELECT 
   NotebookID, CompanyName, StarRating, VacancyID, VacancyName, IsByManager, ResponseCount, KeyPZIDs, SearchPZIDs, ProfLevel, City, OblCenter, Manager /*, QuestionID, QuestionName*/
 , [rabota.ua] AS 'rabota.ua'
 , [work.ua] AS 'work.ua'
 , [hh.ua] AS 'hh.ua'
 , [slando.ua] AS 'slando.ua'
FROM 
(
SELECT NotebookID, CompanyName, StarRating, VacancyID, VacancyName, IsByManager, ResponseCount, KeyPZIDs, SearchPZIDs, ProfLevel, City, OblCenter, Manager, Answer1Level, Answer2Level
FROM C
WHERE EXISTS (SELECT * FROM C C2 WHERE C2.VacancyAnketaID = C.VacancyAnketaID AND C2.Answer1Level = CASE WHEN @WorkEvaluated = 1 THEN 'work.ua' ELSE C2.Answer1Level END)
 AND EXISTS (SELECT * FROM C C3 WHERE C3.VacancyAnketaID = C.VacancyAnketaID AND C3.Answer1Level = CASE WHEN @RabotaEvaluated = 1 THEN 'rabota.ua' ELSE C3.Answer1Level END)
) AS SourceTable
PIVOT
(
MIN(Answer2Level)
FOR Answer1Level IN ([rabota.ua],[work.ua],[hh.ua],[slando.ua])
) AS PivotTable
 )
 , C3 AS
 (
SELECT 'rabota.ua' AS Source, [rabota.ua] AS Satisfaction, COUNT(*) AS AnsNum, SUM(COUNT(*)) OVER() AS TotalNum, 1. * COUNT(*) / SUM(COUNT(*)) OVER() AS Weight
FROM C2
WHERE [rabota.ua] IS NOT NULL
GROUP BY [rabota.ua]
UNION ALL
SELECT 'work.ua' AS Source, [work.ua] AS Satisfaction, COUNT(*), SUM(COUNT(*)) OVER(), 1. * COUNT(*) / SUM(COUNT(*)) OVER()
FROM C2
WHERE [work.ua] IS NOT NULL
GROUP BY [work.ua]
UNION ALL
SELECT 'hh.ua' AS Source, [hh.ua] AS Satisfaction, COUNT(*), SUM(COUNT(*)) OVER(), 1. * COUNT(*) / SUM(COUNT(*)) OVER()
FROM C2
WHERE [hh.ua] IS NOT NULL
GROUP BY [hh.ua]
UNION ALL
SELECT 'slando.ua' AS Source, [slando.ua] AS Satisfaction, COUNT(*), SUM(COUNT(*)) OVER(), 1. * COUNT(*) / SUM(COUNT(*)) OVER()
FROM C2
WHERE [slando.ua] IS NOT NULL
GROUP BY [slando.ua]
 )
SELECT 'Отлично + Очень хорошо' AS NpsType, Source, SUM(Weight) AS NPS
FROM C3
WHERE Satisfaction IN ('Отлично','Очень хорошо')
GROUP BY Source

UNION ALL

SELECT 'Отлично + Очень хорошо + Хорошо' AS NpsType, Source, SUM(Weight) AS NPS
FROM C3
WHERE Satisfaction IN ('Отлично','Очень хорошо','Хорошо')
GROUP BY Source

UNION ALL

SELECT 'Отлично + Очень хорошо - Плохо - Удовлетворительно' AS NpsType, Source
 , SUM(Weight)
   - ISNULL((SELECT SUM(Weight)
	  FROM C3 C30
	  WHERE Satisfaction IN ('Плохо','Удовлетворительно')
	   AND C30.Source = C3.Source
     ),0) AS NPS
FROM C3
WHERE Satisfaction IN ('Отлично','Очень хорошо')
GROUP BY Source</CommandText>
      </Query>
      <Fields>
        <Field Name="NpsType">
          <DataField>NpsType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Source">
          <DataField>Source</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NPS">
          <DataField>NPS</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DStParentGroups">
      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <CommandText>SELECT        ParentGroupID, ParentGroupName
FROM            (SELECT        ParentGroupID, ParentGroupName
                          FROM            Reporting.dbo.Lookup_ParentGroups
                          UNION ALL
                          SELECT        0 AS Expr1, 'Все профессии' AS Expr2) AS A
ORDER BY CASE ParentGroupID WHEN 0 THEN '0' ELSE ParentGroupName END</CommandText>
