    <DataSet Name="DStAnketaCities">
      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <CommandText>SELECT DISTINCT C.Id, C.Name
 , CASE 
    WHEN Name = 'Киев' THEN 'Киев'
	WHEN Name IN ('Александрия','Борисполь','Боярка','Бровары','Васильков','Вишневое','Вышгород','Ирпень','Ковель','Коростень','Лубны','Нежин','Новомосковск','Обухов','Переяслав-Хмельницкий',
				  'Прилуки','Трускавец','Фастов','Шостка','Энергодар','Белая Церковь','Бердянск','Кировоград','Кривой Рог','Лисичанск','Мариуполь','Харьков','Мелитополь','Никополь') THEN '+ 15'
	WHEN Name IN ('Буча','Измаил','Ильичевск','Смела','Стрый','Умань','Червоноград','Алчевск','Житомир','Запорожье','Краматорск','Львов','Николаев','Полтава','Черновцы','Павлоград') THEN '0 - 15'
	WHEN Name IN ('Винница','Кременчуг','Одесса','Сумы','Херсон','Новая Каховка','Черкассы','Чернигов') THEN '-15 - 0'
	WHEN Name IN ('Симферополь','Евпатория','Севастополь','Ялта','Феодосия','Керчь') THEN 'Крым'
	WHEN Name IN ('Луганск','Донецк','Славянск','Северодонецк','Макеевка','Красноармейск','Горловка') THEN 'Проблемные'
	WHEN Name IN ('Луцк','Ровно','Ивано-Франковск','Тернополь','Ужгород','Мукачево','Хмельницкий','Каменец-Подольский') THEN 'Западная Украина'
	WHEN Name IN ('Днепродзержинск','Днепропетровск') THEN 'Бастилия'
	ELSE 'Другое'
   END AS SpiderDiffCityGroup
FROM dbo.CRM_VacancyAnketa VA
 JOIN dbo.City C ON VA.CityIDs = C.Id
ORDER BY C.Id</CommandText>
