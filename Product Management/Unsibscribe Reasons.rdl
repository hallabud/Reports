  <DataSets>
    <DataSet Name="UnsubscribeReasons">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
   ReasonID
 , CASE ReasonID
	WHEN 1 THEN 'Нашел работу'
	WHEN 2 THEN 'Получаю слишком много писем'
	WHEN 3 THEN 'Другая причина'
   END AS ReasonName
 , Text
 , CASE 
    WHEN Text = '' THEN NULL
	ELSE Email
   END AS UnsubscribeEmail
 , COUNT(DISTINCT Email) AS EmailCount
FROM SRV16.RabotaUA2.dbo.UnsubscribeReason WITH (NOLOCK)
WHERE AddDate BETWEEN @StartDate AND @EndDate
GROUP BY ReasonID, Text
 , CASE 
    WHEN Text = '' THEN NULL
	ELSE Email
   END
ORDER BY ReasonID</CommandText>
      </Query>
      <Fields>
        <Field Name="ReasonID">
          <DataField>ReasonID</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="ReasonName">
          <DataField>ReasonName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EmailCount">
          <DataField>EmailCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Text">
          <DataField>Text</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UnsubscribeEmail">
          <DataField>UnsubscribeEmail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
