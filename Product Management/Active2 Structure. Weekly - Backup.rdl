        <CommandText>DECLARE @TodayDate DATETIME; SET @TodayDate = dbo.fnGetDatePart(GETDATE());

WITH CTE AS 
 (
SELECT  
   YearNum
 , MonthNum
 , MonthNameRus
 , FullDate + 1 AS FullDate
 , DayNum
 , WeekDayNum
 , IsMegaChecked
 , HasPaidServices
 , MonthsFromRegDate
 , IsActive2
 , CASE 
    WHEN IsActive2 = 0 THEN 'Не Активная 2'
	WHEN MonthsFromRegDate &lt;= 2 THEN 'Новая'
    WHEN EXISTS (SELECT * 
	FROM dbo.vw_CompaniesActivityGroups_Active2 vw2
	 WHERE vw2.Company_key = vw.Company_key
                  AND vw2.FullDate = DATEADD(MONTH, -1, vw.FullDate)
	  AND vw2.IsActive2 = 1) THEN 'Удержанная'
    ELSE 'Возвращенная'
   END AS 'CompanyGrp'
 , Company_key
FROM dbo.vw_CompaniesActivityGroups_Active2 vw
WHERE IsMegachecked = 1 OR HasPaidServices = 1
 )
, CTE_Companies AS 
 (
SELECT
   YearNum
 , MonthNum
 , MonthNameRus
 , FullDate
 , DayNum
 , IsMegaChecked
 , HasPaidServices
 , MonthsFromRegDate
 , IsActive2
 , CompanyGrp
 , Company_key
 , ROW_NUMBER() OVER(PARTITION BY Company_key ORDER BY FullDate) AS DateOrdNum
FROM CTE
WHERE WeekDayNum = 5 OR FullDate = @TodayDate
 )
, CTE_Final AS
 (
SELECT
   C1.YearNum
 , C1.MonthNum
 , C1.MonthNameRus
 , C1.FullDate
 , C1.DayNum
 , C1.IsMegaChecked
 , C1.HasPaidServices
 , C1.MonthsFromRegDate
 , C1.IsActive2
 , CASE
    WHEN C1.IsActive2 = 0 AND C2.IsActive2 = 1 THEN 'Отвалившаяся'
    ELSE C1.CompanyGrp
   END AS 'CompanyGrp'
 , C2.CompanyGrp AS PrevCompanyGrp
 , C1.Company_key
FROM CTE_Companies AS C1
 LEFT JOIN CTE_Companies AS C2 ON C1.Company_key = C2.Company_key AND C1.DateOrdNum = C2.DateOrdNum + 1
 )
SELECT
   YearNum
 , MonthNum
 , MonthNameRus
 , FullDate
 , DayNum
 , IsMegaChecked
 , HasPaidServices
-- , MonthsFromRegDate -- TODO: не забыть убрать при группировке
 , IsActive2
 , CompanyGrp 
 , PrevCompanyGrp
-- , Company_key
 , COUNT(DISTINCT Company_key) AS CompaniesNum
FROM CTE_Final
WHERE FullDate &gt;= '2013-04-01'
 AND CompanyGrp = 'Отвалившаяся'
GROUP BY 
   YearNum
 , MonthNum
 , MonthNameRus
 , FullDate
 , DayNum
 , IsMegaChecked
 , HasPaidServices
 , IsActive2
 , CompanyGrp 
 , PrevCompanyGrp
;</CommandText>
