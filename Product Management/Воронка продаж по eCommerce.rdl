  <DataSets>
    <DataSet Name="DStEcommerceDealsTotals">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH DealsAll AS
 (
SELECT 
   DRST.Name AS DealState
 , DRT.Name AS DealType
 , DL.ID AS DealID
 , DL.NotebookID
 , D.FullDate
 , O.ID AS OrderID
 , COALESCE(M.Id, 1) AS ManagerID
 , COALESCE(M.Name, 'rabota.ua') AS ManagerName
 , OAP.PaySum
 , OAP.PayDate
 , LAG(PaySum, 1, 0) OVER(PARTITION BY O.NotebookID ORDER BY OAP.PayDate) AS PrvPaySum
 , LAG(PayDate, 1, 0) OVER(PARTITION BY O.NotebookID ORDER BY OAP.PayDate) AS PrvPayDate
FROM SRV16.RabotaUA2.dbo.CRM_Deal DL WITH (NOLOCK)
 JOIN SRV16.RabotaUA2.dbo.CRM_Directory DRST WITH (NOLOCK) ON DL.StateID = DRST.ID AND DRST.Type = 'DealState'
 JOIN SRV16.RabotaUA2.dbo.CRM_Directory DRT WITH (NOLOCK) ON DL.TypeID = DRT.ID AND DRT.Type = 'DealType'
 JOIN Reporting.dbo.DimDate D WITH (NOLOCK) ON CONVERT(DATE, DL.AddDate) = D.FullDate
 LEFT JOIN SRV16.RabotaUA2.dbo.Orders O WITH (NOLOCK) ON DL.ID = O.DealID AND O.IsDealMain = 1
 LEFT JOIN SRV16.RabotaUA2.dbo.Order_Acc OA WITH (NOLOCK) ON O.ID = OA.OrderID
 LEFT JOIN SRV16.RabotaUA2.dbo.Order_AccPayment OAP WITH (NOLOCK) ON OA.ID = OAP.AccID AND OA.Year = OAP.AccYear
 LEFT JOIN SRV16.RabotaUA2.dbo.aspnet_Membership MMB WITH (NOLOCK) ON NULLIF(DL.LoginEMail_DealOwner, '') = MMB.Email
 LEFT JOIN SRV16.RabotaUA2.dbo.Manager M ON MMB.UserId = M.aspnet_UserUIN
 --AND M.ID IN (@ManagerIDs)
 )
 , EcommerceDealsGroups AS
 (
SELECT 1 AS OrderNumber, 'Все сделки eCommerce' AS DealsGroupName, COUNT(DISTINCT DealID) AS DealsCount
FROM DealsAll
WHERE DealType = 'eCommerce'
 AND FullDate BETWEEN @StartDate AND @EndDate 
 
UNION ALL

SELECT 2 AS OrderNumber, 'Реализованные сделки' AS DealsGroupName, COUNT(DISTINCT DealID)
FROM DealsAll
WHERE DealType = 'eCommerce' AND DealState = 'Реализованная'
 AND FullDate BETWEEN @StartDate AND @EndDate 

UNION ALL

SELECT 3 AS OrderNumber, 'Оплаченные сделки' AS DealsGroupName, COUNT(DISTINCT DealID)
FROM DealsAll
WHERE DealType = 'eCommerce' AND PaySum &gt; 0
 AND FullDate BETWEEN @StartDate AND @EndDate 

UNION ALL

SELECT 4 AS OrderNumber, 'Сделки от регулярных покупателей' AS DealsGroupName, COUNT(DISTINCT DealID)
FROM DealsAll
WHERE DealType = 'eCommerce' AND PaySum &gt; 0 AND PrvPaySum &gt; 0 AND PrvPayDate &gt; DATEADD(YEAR, -1, PayDate)
 AND FullDate BETWEEN @StartDate AND @EndDate 
 )

SELECT OrderNumber, DealsGroupName, DealsCount, CAST(100. * DealsCount / FIRST_VALUE(DealsCount) OVER(ORDER BY OrderNumber) AS INT) AS WeightInTotal
FROM EcommerceDealsGroups
;</CommandText>
      </Query>
      <Fields>
        <Field Name="OrderNumber">
          <DataField>OrderNumber</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DealsGroupName">
          <DataField>DealsGroupName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DealsCount">
          <DataField>DealsCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="WeightInTotal">
          <DataField>WeightInTotal</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
