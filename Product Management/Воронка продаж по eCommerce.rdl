SELECT OrderNumber, DealsGroupName, YearNum, MonthNum, MonthNameRus, COUNT(DISTINCT DealID) AS DealsCount
 , 1. * COUNT(DISTINCT DealID) / FIRST_VALUE(COUNT(DISTINCT DealID)) OVER(PARTITION BY YearNum, MonthNum ORDER BY OrderNumber) AS WeightInTotal
FROM EcommerceDealsGroups
WHERE FullDate BETWEEN @StartDate AND @EndDate 
GROUP BY OrderNumber, DealsGroupName, YearNum, MonthNum, MonthNameRus;</CommandText>
