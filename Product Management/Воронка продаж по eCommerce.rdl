 LEFT JOIN SRV16.RabotaUA2.dbo.Orders O WITH (NOLOCK) ON DL.ID = O.DealID AND O.IsDealMain = 1
 LEFT JOIN SRV16.RabotaUA2.dbo.Order_Acc OA WITH (NOLOCK) ON O.ID = OA.OrderID
 LEFT JOIN SRV16.RabotaUA2.dbo.Order_AccPayment OAP WITH (NOLOCK) ON OA.ID = OAP.AccID AND OA.Year = OAP.AccYear
 LEFT JOIN SRV16.RabotaUA2.dbo.aspnet_Membership MMB WITH (NOLOCK) ON NULLIF(DL.LoginEMail_DealOwner, '') = MMB.Email
 LEFT JOIN SRV16.RabotaUA2.dbo.Manager M WITH (NOLOCK) ON MMB.UserId = M.aspnet_UserUIN

