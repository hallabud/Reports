  <DataSets>
    <DataSet Name="DStMain">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <CommandText>SELECT DD.YearNum, DD.WeekNum, DD.WeekName, 'Кол-во новых вакансий' AS Metric, COUNT(DISTINCT NCS.VacancyID) AS Value
FROM Analytics.dbo.NotebookCompany_Spent NCS
 JOIN Analytics.dbo.VacancyExtra VE ON NCS.VacancyID = VE.VacancyID
 JOIN Reporting.dbo.DimDate DD ON dbo.fnGetDatePart(NCS.AddDate) = DD.FullDate
WHERE NCS.AddDate &gt;= '2015-01-01'
 AND VE.IsModerated = 1 AND VE.IsModeratedRubric = 1
 AND NOT EXISTS (SELECT * FROM Analytics.dbo.NotebookCompany_Spent WHERE VacancyID = NCS.VacancyID AND AddDate &lt; NCS.AddDate)
GROUP BY DD.YearNum, DD.WeekNum, DD.WeekName

UNION ALL

SELECT DD.YearNum, DD.WeekNum, DD.WeekName, 'Кол-во переопубликованных вакансий' AS Metric, COUNT(DISTINCT NCS.VacancyID) AS Value
FROM Analytics.dbo.NotebookCompany_Spent NCS
 JOIN Analytics.dbo.VacancyExtra VE ON NCS.VacancyID = VE.VacancyID
 JOIN Reporting.dbo.DimDate DD ON dbo.fnGetDatePart(NCS.AddDate) = DD.FullDate
WHERE NCS.AddDate &gt;= '2015-01-01'
 AND VE.IsModerated = 1 AND VE.IsModeratedRubric = 1
 AND EXISTS (SELECT * FROM Analytics.dbo.NotebookCompany_Spent WHERE VacancyID = NCS.VacancyID AND AddDate &lt; NCS.AddDate)
GROUP BY DD.YearNum, DD.WeekNum, DD.WeekName

UNION ALL

SELECT DD.YearNum, DD.WeekNum, DD.WeekName, 'Кол-во активных компаний', COUNT(DISTINCT Company_key)
FROM Reporting.dbo.FactCompanyStatuses FCS
 JOIN Reporting.dbo.DimDate DD ON FCS.Date_key = DD.Date_key
WHERE DD.FullDate &gt;= '2015-01-01'
 AND FCS.VacancyNum &gt; 0
GROUP BY DD.YearNum, DD.WeekNum, DD.WeekName

UNION ALL

SELECT DD.YearNum, DD.WeekNum, DD.WeekName, 'Кол-во платящих компаний', COUNT(DISTINCT NotebookID)
FROM Reporting.dbo.FactRecognizedRevenueNotebook FRRN
 JOIN Reporting.dbo.DimDate DD ON FRRN.Date_key = DD.Date_key
WHERE DD.FullDate &gt;= '2015-01-01'
GROUP BY DD.YearNum, DD.WeekNum, DD.WeekName

UNION ALL

SELECT DD.YearNum, DD.WeekNum, DD.WeekName, 'Кол-во заказов платных услуг с сайта', COUNT(*)
FROM Analytics.dbo.Orders O
 JOIN Reporting.dbo.DimDate DD ON dbo.fnGetDatePart(O.AddDate) = DD.FullDate
WHERE O.AddDate &gt;= '2015-01-01'
 AND O.Type = 5
 AND O.State IN (4,5)
GROUP BY DD.YearNum, DD.WeekNum, DD.WeekName

UNION ALL

SELECT DD.YearNum, DD.WeekNum, DD.WeekName, 'Кол-во заказов платных услуг у менеджеров', COUNT(*)
FROM Analytics.dbo.Orders O
 JOIN Reporting.dbo.DimDate DD ON dbo.fnGetDatePart(O.AddDate) = DD.FullDate
WHERE O.AddDate &gt;= '2015-01-01'
 AND O.Type = 1
 AND O.State IN (4,5)
GROUP BY DD.YearNum, DD.WeekNum, DD.WeekName

UNION ALL

SELECT DD.YearNum, DD.WeekNum, DD.WeekName, 'Кол-во обращений за помощью', COUNT(*) 
FROM Analytics.dbo.Admin7_Request R
 JOIN Reporting.dbo.DimDate DD ON dbo.fnGetDatePart(R.AddDate) = DD.FullDate
WHERE R.AddDate &gt;= '2015-01-01'
GROUP BY DD.YearNum, DD.WeekNum, DD.WeekName</CommandText>
      </Query>
