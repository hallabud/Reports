  <DataSources>
    <DataSource Name="DSrcReporting">
      <Transaction>true</Transaction>
      <DataSourceReference>DSrcReporting</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>7655bd45-4ef1-4adf-8b8b-d5e2c4db6c33</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DStMain">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@N">
            <Value>=Parameters!N.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @Date_key INT
SELECT @Date_key = Date_key FROM dbo.DimDate WHERE FullDate = dbo.fnGetDatePart(GETDATE());

--DECLARE @N INT;
--SET @N = 100;

WITH C AS
 (
SELECT 
   0 AS Source
 , DC.NotebookId AS CompanyID
 , DC.CompanyName
 , DC.AgeGroup
 , DC.WorkConnectionGroup AS ConnectionGroup
 , DC.VacancyNum AS RabotaVacancyNum
 , ISNULL(DC.WorkVacancyNum,0) AS WorkVacancyNum
 , DC.VacancyNumMonth1
 , DC.VacancyNumMonth2
 , DC.VacancyNumMonth3
 , DC.WorkVacancyNumMonth1
 , DC.WorkVacancyNumMonth2
 , DC.WorkVacancyNumMonth3
 , CAST(dbo.fnGetMaximumOf2Values(DC.VacancyNumMonth1, DC.WorkVacancyNumMonth1)  + dbo.fnGetMaximumOf2Values(DC.VacancyNumMonth2, DC.WorkVacancyNumMonth2) + dbo.fnGetMaximumOf2Values(DC.VacancyNumMonth3, DC.WorkVacancyNumMonth3) AS INT) AS VacancyNumQuarter
 , DC.AvgLast3Month
 , DC.IndexActivity
 , DC.IndexAttraction
 , DC.StarRating
 , CASE WHEN DC.CompanyState IN ('МЕГА проверенный') THEN 'МЕГА' ELSE 'неМЕГА' END AS CompanyStateGroup
 , CASE WHEN dbo.udf_GetCompanyAgeGroup(dbo.fnGetMinimumOf2ValuesDate(A.AddDate, ISNULL(DC.FirstPubDate, GETDATE()))) /*DC.AgeGroup*/ IN ('Подростки','Взрослые') THEN 'Деды' ELSE 'Салаги' END AS AgeGroupGroup
FROM dbo.DimCompany DC
 LEFT JOIN 
    (SELECT CompanyID, AddDate
	 FROM Analytics.dbo.SpiderCompany SC 
	 WHERE SC.Source = 1 AND NotebookId IS NOT NULL) A ON DC.WorkCompanyID = A.CompanyId
WHERE /*DC.CompanyState IN ('МЕГА проверенный')
 AND DC.AgeGroup IN ('Подростки','Взрослые')
 AND*/ DC.IndexAttraction &gt; 0

UNION ALL

SELECT   
   1 AS Source
 , DSC.CompanyId
 , DSC.CompanyName
 , dbo.udf_GetCompanyAgeGroup(SC.AddDate) AS AgeGroup
 , 'Компании на work.ua без привязки к rabota.ua'
 , 0
 , FSCI.VacancyCount
 , 0
 , 0
 , 0
 , DSC.VacancyNumMonth1
 , DSC.VacancyNumMonth2
 , DSC.VacancyNumMonth3
 , DSC.VacancyNumMonth1 + DSC.VacancyNumMonth2 + DSC.VacancyNumMonth3 AS VacancyNumQuarter
 , DSC.AvgLast3Month
 , DSC.IndexActivity
 , DSC.IndexAttraction
 , DSC.StarRating
 , CASE WHEN DSC.IsApproved = 1 THEN 'МЕГА' ELSE 'неМЕГА' END AS CompanyStateGroup
 , CASE WHEN dbo.udf_GetCompanyAgeGroup(SC.AddDate) IN ('Подростки','Взрослые') THEN 'Деды' ELSE 'Салаги' END AS AgeGroupGroup
FROM dbo.DimSpiderCompany DSC
 JOIN Analytics.dbo.SpiderCompany SC ON DSC.CompanyID = SC.CompanyID AND SC.Source = 1
 JOIN dbo.FactSpiderCompanyIndexes FSCI ON DSC.SpiderCompanyID = FSCI.SpiderCompanyID AND FSCI.Date_key = @Date_key
WHERE DSC.NotebookId IS NULL
 --AND dbo.udf_GetCompanyAgeGroup(SC.AddDate) IN ('Подростки','Взрослые')
 AND IndexAttraction &gt; 0
 --AND DSC.IsApproved = 1
 )
 , C2 AS
 (
SELECT TOP (@N) PERCENT * 
FROM C
ORDER BY IndexAttraction DESC, AvgLast3Month DESC
 )
SELECT ConnectionGroup, CompanyStateGroup, AgeGroupGroup, COUNT(*) AS CompanyNum, SUM(VacancyNumQuarter) AS VacancyNumQuarter
FROM C2
GROUP BY ConnectionGroup, CompanyStateGroup, AgeGroupGroup
UNION ALL
SELECT 'Total Market', CompanyStateGroup, AgeGroupGroup, COUNT(*), SUM(VacancyNumQuarter)
FROM C
GROUP BY CompanyStateGroup, AgeGroupGroup</CommandText>
      </Query>
