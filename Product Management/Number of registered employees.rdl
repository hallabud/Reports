  <DataSets>
    <DataSet Name="DStRegisteredEmployees">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @EndDate DATETIME;
SET @EndDate = Analytics.dbo.fnGetDatePart(GETDATE());

SELECT 
   D.FullDate
 , (SELECT COUNT(*) FROM Analytics.dbo.NotebookEmployee NE WHERE NE.AddDate &lt;= D.FullDate) AS EmployeesNum
FROM dbo.DimDate D
WHERE D.IsLastDayOfMonth = 1
 AND D.FullDate BETWEEN @StartDate AND @EndDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="FullDate">
          <DataField>FullDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="EmployeesNum">
          <DataField>EmployeesNum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="StartDate">
      <DataType>DateTime</DataType>
      <DefaultValue>
        <Values>
          <Value>=DateAdd("m", -19, Today())</Value>
        </Values>
      </DefaultValue>
      <Prompt>Укажите начальную дату:</Prompt>
    </ReportParameter>
  </ReportParameters>
