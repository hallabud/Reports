      <Fields>
        <Field Name="YearNum">
          <DataField>YearNum</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="WeekNum">
          <DataField>WeekNum</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="WeekName">
          <DataField>WeekName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CityGroup">
          <DataField>CityGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CityNameRus">
          <DataField>CityNameRus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AvgRabota">
          <DataField>AvgRabota</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AvgWork">
          <DataField>AvgWork</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <CommandText>WITH C AS
 (
SELECT D.FullDate, D.WeekNum, D.WeekName, D.MonthNum, D.MonthNameRus, D.YearNum
 , CASE Source WHEN 1 THEN 'work.ua' WHEN 0 THEN 'rabota.ua' END AS Source
 , CASE 
    WHEN Name = 'Киев' THEN 'Киев'
	WHEN Name IN ('Днепропетровск', 'Донецк', 'Одесса', 'Харьков', 'Львов', 'Запорожье') THEN 'Крупные города'
    ELSE 'Другие' END AS CityGroup
 , Name AS CityNameRus
 , VacancyCount
FROM Analytics.dbo.Spider2VacancyCountByCity S2V
 JOIN Reporting.dbo.DimDate D ON S2V.Date = D.FullDate
 JOIN Analytics.dbo.City C ON S2V.CityID = C.Id
 )
 , C_Pivoted AS
 (
SELECT FullDate, WeekNum, WeekName, MonthNum, MonthNameRus, YearNum, CityGroup, CityNameRus
 , [rabota.ua] AS 'rabota.ua'
 , [work.ua] AS 'work.ua'
-- , ([rabota.ua] - [work.ua]) / Reporting.dbo.fnGetMinimumOf2Values([rabota.ua],[work.ua]) AS 'GAP'
FROM 
(SELECT *
FROM C) p
PIVOT
(
SUM(VacancyCount)
FOR Source IN ([rabota.ua],[work.ua])
) AS pvt
 )
SELECT YearNum, WeekNum, WeekName, CityGroup, CityNameRus, AVG([rabota.ua]) AS AvgRabota, AVG([work.ua]) AS AvgWork
FROM C_Pivoted
GROUP BY YearNum, WeekNum, WeekName, CityGroup, CityNameRus;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
