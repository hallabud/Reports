  <DataSets>
    <DataSet Name="DStVacancyCountByCity">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

WITH C AS
 (
SELECT D.FullDate, D.WeekNum, D.WeekName, D.MonthNum, D.MonthNameRus, D.YearNum
 , CASE Source WHEN 1 THEN 'work.ua' WHEN 0 THEN 'rabota.ua' END AS Source
 , CASE 
    WHEN Name = 'Киев' THEN 'Киев'
	WHEN Name IN ('Днепропетровск', 'Донецк', 'Одесса', 'Харьков', 'Львов', 'Запорожье') THEN 'Крупные города'
	WHEN Name IN ('Ужгород','Ивано-Франковск','Тернополь','Черновцы','Луцк','Хмельницкий','Ровно') THEN 'Западные города'
	WHEN C.OblastCityID IS NULL OR C.Name = 'Севастополь' THEN 'Областные центры'
    ELSE 'Другие' END AS CityGroup
 , Name AS CityNameRus
 , VacancyCount
FROM SRV16.RabotaUA2.dbo.Spider2VacancyCountByCity S2V
 JOIN Reporting.dbo.DimDate D ON dbo.fnGetDatePart(S2V.Date) = D.FullDate
 JOIN SRV16.RabotaUA2.dbo.City C ON S2V.CityID = C.Id
WHERE D.FullDate BETWEEN @StartDate AND @EndDate

UNION ALL

SELECT D.FullDate, D.WeekNum, D.WeekName, D.MonthNum, D.MonthNameRus, D.YearNum
 , CASE Source WHEN 1 THEN 'work.ua' WHEN 0 THEN 'rabota.ua' END AS Source
 , 'Всего' AS CityGroup
 , 'Все регионы' AS CityNameRus
 , VacancyCount
FROM SRV16.RabotaUA2.dbo.Spider2VacancyCountByCity S2V
 JOIN Reporting.dbo.DimDate D ON dbo.fnGetDatePart(S2V.Date) = D.FullDate
WHERE CityID = 0 AND D.FullDate BETWEEN @StartDate AND @EndDate
 )
 , C_Pivoted AS
 (
SELECT FullDate, WeekNum, WeekName, MonthNum, MonthNameRus, YearNum, CityGroup, CityNameRus
 , [rabota.ua] AS 'rabota.ua'
 , [work.ua] AS 'work.ua'
-- , ([rabota.ua] - [work.ua]) / Reporting.dbo.fnGetMinimumOf2Values([rabota.ua],[work.ua]) AS 'GAP'
FROM 
(SELECT *
FROM C) p
PIVOT
(
SUM(VacancyCount)
FOR Source IN ([rabota.ua],[work.ua])
) AS pvt
 )
SELECT YearNum, WeekNum, WeekName, CityGroup, CityNameRus, AVG([rabota.ua]) AS AvgRabota, AVG([work.ua]) AS AvgWork
FROM C_Pivoted
GROUP BY YearNum, WeekNum, WeekName, CityGroup, CityNameRus;</CommandText>
