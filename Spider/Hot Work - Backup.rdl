    <DataSet Name="Analytics">
      <Fields>
        <Field Name="CompanyID">
          <DataField>CompanyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="HotVacancyCount">
          <DataField>HotVacancyCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSrcAnalytics</DataSourceName>
        <CommandText>WITH C_Dates AS
 (
SELECT DISTINCT TOP 30 Date
FROM dbo.SpiderCompanyWorkHistory
ORDER BY Date DESC
 )
, C_Companies AS 
 (
SELECT DISTINCT CompanyID
FROM dbo.SpiderCompanyWorkHistory
WHERE HotVacancyCount &gt; 1
 AND Date &gt;= (SELECT MIN(Date) FROM C_Dates)
 )
SELECT SCWH.CompanyID, SC.Name, SCWH.Date, SCWH.HotVacancyCount
FROM dbo.SpiderCompanyWorkHistory SCWH
 JOIN dbo.SpiderCompany SC ON SCWH.CompanyID = SC.CompanyID AND SC.Source = 1
WHERE SCWH.CompanyID IN (SELECT CompanyID FROM C_Companies)
 AND Date IN (SELECT Date FROM C_Dates)
ORDER BY SCWH.CompanyID, Date</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
