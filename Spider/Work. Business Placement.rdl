  <DataSets>
    <DataSet Name="DStMain">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DD.FullDate, DSC.ConnectionGroup, COUNT(*) AS CompanyNum, SUM(FSCI.VacancyCount) AS VacancyNum
FROM dbo.DimSpiderCompany DSC
 JOIN dbo.FactSpiderCompanyIndexes FSCI ON DSC.SpiderCompanyID = FSCI.SpiderCompanyID
 JOIN dbo.DimDate DD ON FSCI.Date_key = DD.Date_key
WHERE DSC.SpiderSource_key = 1
 AND FSCI.IsBusiness IS NOT NULL
 AND FSCI.IsBusiness = 1
 AND DD.FullDate BETWEEN @StartDate AND @EndDate
GROUP BY DD.FullDate, DSC.ConnectionGroup, IsBusiness
ORDER BY FullDate, DSC.ConnectionGroup, IsBusiness</CommandText>
      </Query>
      <Fields>
        <Field Name="FullDate">
          <DataField>FullDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CompanyNum">
          <DataField>CompanyNum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ConnectionGroup">
          <DataField>ConnectionGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="VacancyNum">
          <DataField>VacancyNum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DStIsBusinessGroups">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH C AS
 (
SELECT
   DD.FullDate
 , DSC.SpiderCompanyID
 , FSCI.IsBusiness AS IsBusiness_Today
 , FSCI_Y.IsBusiness AS IsBusiness_Yesterday
 , ISNULL((SELECT DISTINCT 1 FROM dbo.FactSpiderCompanyIndexes WHERE SpiderCompanyID = FSCI.SpiderCompanyID AND Date_key &lt; FSCI.Date_key AND IsBusiness = 1),0) AS IsBusiness_Before
FROM dbo.DimSpiderCompany DSC
 JOIN dbo.FactSpiderCompanyIndexes FSCI ON DSC.SpiderCompanyID = FSCI.SpiderCompanyID
 JOIN dbo.DimDate DD ON FSCI.Date_key = DD.Date_key
 LEFT JOIN dbo.FactSpiderCompanyIndexes FSCI_Y ON DSC.SpiderCompanyID = FSCI_Y.SpiderCompanyID AND FSCI_Y.Date_key = FSCI.Date_key - 1
WHERE DSC.SpiderSource_key = 1
 AND EXISTS (SELECT * FROM dbo.FactSpiderCompanyIndexes WHERE SpiderCompanyID = FSCI.SpiderCompanyID AND IsBusiness = 1)
 AND DD.FullDate BETWEEN @StartDate AND @EndDate
 )

SELECT 
   FullDate
 , CASE 
    WHEN IsBusiness_Today = 1 AND IsBusiness_Before = 0 THEN 'Новые'
	WHEN IsBusiness_Today = 1 AND IsBusiness_Before = 1 AND IsBusiness_Yesterday = 0 THEN 'Вернувшиеся'
	WHEN IsBusiness_Today = 1 AND IsBusiness_Before = 1 AND IsBusiness_Yesterday = 1 THEN 'Оставшиеся'
	WHEN IsBusiness_Today = 0 AND IsBusiness_Yesterday = 1 THEN 'Потерявшиеся'
    ELSE 'Никогда не находившиеся'
   END AS IsBusinessGroup
 , COUNT(*) AS CompanyNum
FROM C
GROUP BY FullDate
 , CASE 
    WHEN IsBusiness_Today = 1 AND IsBusiness_Before = 0 THEN 'Новые'
	WHEN IsBusiness_Today = 1 AND IsBusiness_Before = 1 AND IsBusiness_Yesterday = 0 THEN 'Вернувшиеся'
	WHEN IsBusiness_Today = 1 AND IsBusiness_Before = 1 AND IsBusiness_Yesterday = 1 THEN 'Оставшиеся'
	WHEN IsBusiness_Today = 0 AND IsBusiness_Yesterday = 1 THEN 'Потерявшиеся'
    ELSE 'Никогда не находившиеся'
   END
 ORDER BY FullDate, IsBusinessGroup
</CommandText>
      </Query>
      <Fields>
        <Field Name="FullDate">
          <DataField>FullDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="IsBusinessGroup">
          <DataField>IsBusinessGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompanyNum">
          <DataField>CompanyNum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DStGap">
      <Query>
        <DataSourceName>DSrcReporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH C AS
 (
SELECT DD.FullDate, DD.Date_key, COUNT(*) AS CompanyNum, SUM(FSCI.VacancyCount) AS VacancyNum
FROM dbo.DimSpiderCompany DSC
 JOIN dbo.FactSpiderCompanyIndexes FSCI ON DSC.SpiderCompanyID = FSCI.SpiderCompanyID
 JOIN dbo.DimDate DD ON FSCI.Date_key = DD.Date_key
WHERE DSC.SpiderSource_key = 1
 AND FSCI.IsBusiness IS NOT NULL
 AND FSCI.IsBusiness = 1
 AND DD.FullDate BETWEEN @StartDate AND @EndDate
GROUP BY DD.FullDate, DD.Date_key
 )
 , C2 AS
 (
SELECT * 
 , (SELECT COUNT(*)
	FROM dbo.FactCompanyStatuses FCS
	WHERE FCS.Date_key = C.Date_key
	 AND (FCS.HasPaidPublishedVacs = 1 OR FCS.HasPaidPublicationsLeft = 1 OR FCS.HasHotPublishedVacs = 1 OR FCS.HasHotPublicationsLeft = 1)) AS PaidRabotaCompanyNum
FROM C
 )
SELECT FullDate
 , 1. * (PaidRabotaCompanyNum - CompanyNum) / dbo.fnGetMinimumOf2Values(PaidRabotaCompanyNum, CompanyNum) AS GapCompany
FROM C2
ORDER BY FullDate</CommandText>
      </Query>
      <Fields>
        <Field Name="FullDate">
          <DataField>FullDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="GapCompany">
          <DataField>GapCompany</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
